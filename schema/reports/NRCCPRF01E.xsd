               <xsd:documentation source="FieldText">[d:b]H3. EXHAUST FAN SUMMARY[/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableFanExh" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="FanExh01_HvacSystemName" type="hvac:HvacSystemName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">System ID</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>AirSys:Name Or ZnSys:Name</dtyp:CBECCnrXMLterm>
                                             <dtyp:termConditions>Result is AirSys If AirSys:Type == Exhaust Or ZnSys If ZnSys:Type == Exhaust</dtyp:termConditions>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="FanExh02_ThermalZoneName" type="hvac:ThermalZoneName"  maxOccurs="unbounded">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Zone Name(s)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">This is a list of all thermal zones served by the exhaust fan system.  It can either be one string with al zone names or multiple entries - one for each zone</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>AirSys:ZnServedAsExhSys Or ZnSys:ZnServedAsExhSys</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="FanExh03_FanCount" type="hvac:FanCount" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Qty</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>AirSys:AirSeg:Fan:SysCnt Or ZnSys:Fan:SysCnt</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="FanExh04_ExhaustFlowRate" type="hvac:ExhaustFlowRate" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">CFM</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>AirSys:AirSeg:Fan:FlowCap Or ZnSys:Fan:FlowCap</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="FanExh05_MotorBHP" type="hvac:MotorBHP" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Motor BHP</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>AirSys:AirSeg:Fan:MtrBHP Or ZnSys:Fan:MtrBHP</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="FanExh06_MotorWatts" type="hvac:MotorWatts" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Power Per Flow (W/cfm)</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>AirSys:AirSeg:Fan:PwrIdx Or ZnSys:Fan:PwrIdx</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="FanExh07_TotalStaticPressure" type="hvac:TotalStaticPressure" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Total Static Pressure (in H2O)</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>AirSys:AirSeg:Fan:TotStaticPress Or ZnSys:Fan:TotStaticPress</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="FanExh08_Status" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Status[d:sup]1[/d:sup]</xsd:documentation>
                                          <xsd:appinfo source="displayterm">
                                             <dtyp:displayterm value="ExistingAlteration">A</dtyp:displayterm>
                                             <dtyp:displayterm value="ExistingUnchanged">E</dtyp:displayterm>
                                             <dtyp:displayterm value="New">N</dtyp:displayterm>
                                          </xsd:appinfo>
                                          
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>AirSys:AirSeg:Fan:Status Or ZnSys:AirSeg:Fan:Status</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="com:Status">
                                             <xsd:enumeration value="ExistingAlteration"/>
                                             <xsd:enumeration value="ExistingUnchanged"/>
                                             <xsd:enumeration value="New"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="FanExhFootNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:i][d:sup]1[/d:sup] Status: N - New, A - Altered, E - Existing[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_HvacWet" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]H4. WET SYSTEM EQUIPMENT (boilers, chillers, cooling towers, etc.) [/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableHvacWet" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="HvacWet01_HvacEquipmentName" type="hvac:HvacEquipmentName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Name or Item Tag</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">This is a string that identifies either    boiler, chiller, heat rejection or thermal energy storage equipment</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>This is a string that identifies either    Boiler:Name, Chiller:Name, HeatRejection:Name or ThermalEnergyStorage:Name</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="HvacWet02_BoilerType" type="hvac:BoilerType" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Equipment Type</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>Blr:Type</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="HvacWet02_ChillerType" type="hvac:ChillerType" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Equipment Type</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>Chlr:Type</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="HvacWet02_HeatRejectionType" type="hvac:HeatRejectionType" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Equipment Type</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>HtRej:Type</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="HvacWet02_HeatExchangerType" type="hvac:HeatExchangerType" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Equipment Type</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="HvacWet02_ThermalEnergyStorageType" type="hvac:ThermalEnergyStorageType" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Equipment Type</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ThrmlEngyStor:Type</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="HvacWet02a_DischargePriority" type="hvac:DischargePriority"  minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Discharge Priority -</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If HvacWet02_ThermalEnergyStorageType has value, this field is required and displayed in same cell  Else nothing is required or displayed</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>ThrmlEngyStor: DischrgPriority</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="HvacWet03_FluidSystemName" type="hvac:FluidSystemName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Parent Fluid System Name</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>FluidSys:Name</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="HvacWet04_HvacEquipmentCount" type="hvac:HvacEquipmentCount" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Qty</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="HvacWet04_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Qty</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="HvacWet05_TEStorageCapacity" type="hvac:TEStorageCapacity" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Vol (gal)</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If HvacWet02_ThermalEnergyStorageType has value, this field is required Else result = NotApplicable stored in NotApplicableMessage</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ThrmlEngyStor:StorCap</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="HvacWet05_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Vol (gal)</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If HvacWet02_ThermalEnergyStorageType has value, this field is required Else result = NotApplicable stored in NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="HvacWet06_CapacityRated" type="hvac:CapacityRated" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rated Capacity</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>Blr:CapRtd</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="HvacWet06_ChillerCapacity" type="hvac:ChillerCapacity" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rated Capacity</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>Chlr:CapRtd</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="HvacWet06_CoolingTowerCapacityRated" type="hvac:CoolingTowerCapacityRated" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rated Capacity</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>HtRej:CapRtd</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="HvacWet06_TERatedCapacity" type="hvac:TERatedCapacity" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rated Capacity</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ThrmlEngyStor:CapRtd</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="HvacWet07_CapacityUnits" type="hvac:CapacityUnits" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Capacity Unit</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="HvacWet08_EfficiencyAFUE" type="hvac:EfficiencyAFUE" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rating</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>Blr:AFUE</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="HvacWet08_ThermalEfficiency" type="hvac:ThermalEfficiency" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rating</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>Blr:ThrmlEff</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="HvacWet08_CombustionEfficiency" type="hvac:CombustionEfficiency" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rating</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>Blr:CombEff</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="HvacWet08_EfficiencyCOP" type="hvac:EfficiencyCOP" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rating</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>Chlr:COP</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="HvacWet08_EfficiencyEER" type="hvac:EfficiencyEER" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rating</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>Chlr:EER</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="HvacWet08_EfficiencyKWperTon" type="hvac:EfficiencyKWperTon" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rating</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>Chlr:KwPerTon</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="HvacWet08_TotalFanHP" type="hvac:TotalFanHP" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rating</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Unit gpm/hp.</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>HtRej:CapRtd / HtRej:TotFanHP</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="HvacWet08_EfficiencyNA" type="comp:EfficiencyNA" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rating</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="HvacWet09_EfficiencyType" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rating Unit</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="hvac:EfficiencyType">
                                                <xsd:enumeration value="AFUE"/>
                                                <xsd:enumeration value="CombustionEfficiency"/>
                                                <xsd:enumeration value="COP"/>
                                                <xsd:enumeration value="EER"/>
                                                <xsd:enumeration value="GalPerMinHp"/>
                                                <xsd:enumeration value="HP"/>
                                                <xsd:enumeration value="KWperTon"/>
                                                <xsd:enumeration value="ThermalEfficiency"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                       <xsd:element name="HvacWet09_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rating Unit</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="HvacWet10_Status" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Status[d:sup]1[/d:sup]</xsd:documentation>
                                          <xsd:appinfo source="displayterm">
                                             <dtyp:displayterm value="ExistingAlteration">A</dtyp:displayterm>
                                             <dtyp:displayterm value="ExistingUnchanged">E</dtyp:displayterm>
                                             <dtyp:displayterm value="New">N</dtyp:displayterm>
                                          </xsd:appinfo>
                                          
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Blr:Status Or Chlr:Status or HtRej:Status</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="com:Status">
                                             <xsd:enumeration value="ExistingAlteration"/>
                                             <xsd:enumeration value="ExistingUnchanged"/>
                                             <xsd:enumeration value="New"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="HvacWetFootNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:i][d:sup]1[/d:sup] Status: N - New, A - Altered, E - Existing[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_Pump" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]H5. PUMPS [/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TablePump" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This table is required if there is a pump included in the project that serves a fluid system or equpipment like boiler, chiller</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Pump01_PumpName" type="hvac:PumpName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Name or Item Tag</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">This is a string that identifies the parent of the Fluid system, Pump: Boiler,Chiller, ThermalEnergyStorage or HeatRejection  that uses the pump</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Pump02_HvacEquipmentName" type="hvac:HvacEquipmentName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Parent Equipment</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">This value is one of the values in HvacWet01_HvacEquipmentName</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>FluidSys:Name, FluidSys:Blr:Name, FluidSys:Chlr:Name, FluidSys:HtRej:Name OR FluidSys:ThrmlEngyStor:Name</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Pump02a_DischargePriority" type="hvac:DischargePriority"  minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Discharge Priority -</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If Pumpt02_ThermalEnergyStorageType has value, this field is required and displayed in same cell  Else nothing is required or displayed</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>FluidSys:ThrmlEngyStor:DischrgPriority</dtyp:CBECCnrXMLterm>
                                             <dtyp:termConditions>If Pumpt02_ThermalEnergyStorageType has value, this field is required and displayed in same cell  Else nothing is required or displayed</dtyp:termConditions>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Pump03_PumpCount" type="hvac:PumpCount" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Qty</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>FluidSys:FluidSeg:Pump:Cnt, FluidSys:Blr:Pump:Cnt,, FluidSys:Chlr:Pump:Cnt, FluidSys:HtRej:Pump:Cnt OR FluidSys:ThrmlEngyStor::Pump:Cnt</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Pump04_PumpPowerUnits" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Power Units</xsd:documentation>
                                          <xsd:appinfo source="displayterm">
                                             <dtyp:displayterm value="BHP">HP</dtyp:displayterm>
                                          </xsd:appinfo>
                                          
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>"HP"</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="hvac:PumpPowerUnits">
                                             <xsd:enumeration value="BHP"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:element name="Pump05_PumpMotorHP" type="hvac:PumpMotorHP" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Power</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>FluidSys:FluidSeg:Pump:MtrHP, FluidSys:Blr:Pump:MtrHP, FluidSys:Chlr:Pump:MtrHP, FluidSys:HtRej:Pump:MtrHP  OR FluidSys:ThrmlEngyStor::Pump:MtrHP</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Pump06_FlowCapacity" type="hvac:FlowCapacity" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">GPM</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>FluidSys:FluidSeg:Pump:FlowCap, FluidSys:Blr:Pump:FlowCap, FluidSys:Chlr:Pump:FlowCap, FluidSys:HtRej:Pump:FlowCap  OR FluidSys:ThrmlEngyStor::Pump:FlowCap</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Pump07_IsVSD" type="hvac:IsVSD" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">VSD</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>FluidSys:FluidSeg:Pump:SpdCtrl , FluidSys:Blr:Pump:SpdCtrl , FluidSys:Chlr:Pump:SpdCtrl , FluidSys:HtRej:Pump:SpdCtrl OR FluidSys:ThrmlEngyStor::Pump:SpdCtrl</dtyp:CBECCnrXMLterm>
                                             <dtyp:termConditions>If Pump:SpdCtrl == VariableSpeed result = true Else If Pump:SpdCtrl == ConstantSpeed result =  false</dtyp:termConditions>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Pump08_Status" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Status[d:sup]1[/d:sup]</xsd:documentation>
                                          <xsd:appinfo source="displayterm">
                                             <dtyp:displayterm value="ExistingAlteration">A</dtyp:displayterm>
                                             <dtyp:displayterm value="ExistingUnchanged">E</dtyp:displayterm>
                                             <dtyp:displayterm value="New">N</dtyp:displayterm>
                                          </xsd:appinfo>
                                          
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>FluidSys:Status, FluidSys:Blr:Status, FluidSys:Chlr:Status, FluidSys:HtRej:Status OR  FluidSys:ThrmlEngyStor:Name</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="com:Status">
                                             <xsd:enumeration value="ExistingAlteration"/>
                                             <xsd:enumeration value="ExistingUnchanged"/>
                                             <xsd:enumeration value="New"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="PumpFootNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:i][d:sup]1[/d:sup] Status: N - New, A - Altered, E - Existing[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_HvacSpec" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]H6. SYSTEM SPECIAL FEATURES [/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableHvacSpec" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="HvacSpec01_HvacSystemName" type="hvac:HvacSystemName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">System Name</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>AirSys:Desc or ZnSys:Desc</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="HvacSpec02_AirSystemType" type="hvac:AirSystemType" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">System Type</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>AirSys:Type</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="HvacSpec02_ZoneSystemType" type="hvac:ZoneSystemType" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">System Type</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ZnSys:Type</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="HvacSpec02_FluidSystemType" type="hvac:FluidSystemType" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">System Type</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>FluidSys:TypeRpt</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="HvacSpec03_OperableWindowsHaveInterlock" type="hvac:OperableWindowsHaveInterlock" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Window Interlocks per [d:t24refnolink/]140.4(n)</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ThrmlZn:OperableWinHaveInterlock</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="HvacSpec03_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Window Interlocks per [d:t24refnolink/]140.4(n)</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="HvacSpec04Choice" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"></xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:complexType>
                                          <xsd:choice >
                                             <xsd:element name="HvacSpec04" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Other Special Features and Controls</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:complexType>
                                                   <xsd:sequence>
                                                      <xsd:element name="HvacSpec04_HvacControls" type="hvac:HvacControls"  minOccurs="0" maxOccurs="unbounded">
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText"></xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">Possible values: DCV, DDC, DualMaxReheat, FID, Fixed2SetpointTempControl, FixedSupplyAirTemp, FixedTempControl, HeatRecovery, OptimumStart, OutsideAirReset, ScheduledTempControl, SingleMaxReheat, SupplyAirTempResetOA, SupplyAirTempScheduled, TempResetWB, VentEquipAccessible, WarmestZoneReset, WarmestZoneResetFlow, WarmestZoneResetTemp, ZoneWithCO2Sensor</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="HvacSpec04_EconomizerControlMethod" type="hvac:EconomizerControlMethod"  minOccurs="0" maxOccurs="unbounded">
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText"></xsd:documentation>
                                                            <xsd:appinfo source="CBECCnrXML">
                                                               <dtyp:CBECCnrXMLterm>AirSys:OACtrl:EconomizerControlMethod</dtyp:CBECCnrXMLterm>
                                                            </xsd:appinfo>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="HvacSpec04_EvaporativeCoolerType" type="hvac:EvaporativeCoolerType"  minOccurs="0" maxOccurs="unbounded">
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText"></xsd:documentation>
                                                            <xsd:appinfo source="CBECCnrXML">
                                                               <dtyp:CBECCnrXMLterm>AirSys:AirSeg:EvapClr:Type</dtyp:CBECCnrXMLterm>
                                                            </xsd:appinfo>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                   </xsd:sequence>
                                                </xsd:complexType>
                                             </xsd:element>
                                             <xsd:element name="HvacSpec04_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Other Special Features and Controls</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                          </xsd:choice>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="HvacSpecEndNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i] Notes: This table includes controls related to the performance path only.  For projects using the prescriptive path, mandatory and prescriptive controls requirements are documented on the NRCC-MCH-E. [/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_VentNR" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]H7. NON-RESIDENTIAL  VENTILATION [/d:b]</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">If project includes ventilation, this table is required If Info13_TopLevelOccupancy != HighRiseResidential And Info13_TopLevelOccupancy != HotelMotel And Info13_TopLevelOccupancy != ResidentialNR</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableVentNR" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Vent01_ThermalZoneName" type="hvac:ThermalZoneName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Zone Name</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>ThrmlZn:Name Or ThrmlZone:Name</dtyp:CBECCnrXMLterm>
                                             <dtyp:termConditions>If ThrmlZn:Type == Conditioned AND ThrmlZn:VentStd == Other result = ThrmlZone:Name ThrmlZn:VentStdDesc + "ventilation"  Else result = ThrmlZone:Name</dtyp:termConditions>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Vent01a_VentilationStandardDescription" type="hvac:VentilationStandardDescription"  minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"></xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If a stantard other than T24 is used, this field contains a description of that standard which is appended to the ThermalZoneName and followed by the word "ventilation"</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>ThrmlZn:VentStdDesc + "ventilation"</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Vent02to06Subheading" minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Mechanical Ventilation</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Vent02_VentilationSpaceFunction" type="com:VentilationSpaceFunction"  maxOccurs="unbounded">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Ventilation Function</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ThrmlZn:VentFuncList</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Vent02_VentilationSpaceFunctions" type="com:VentilationSpaceFunctions" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Ventilation Function</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ThrmlZn:VentFuncList</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="Vent03_OccupancyCount" type="com:OccupancyCount" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"># of People</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>ThrmlZn:OccNumSim</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Vent04_VentSystemOA" type="hvac:VentSystemOA" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Supply OA CFM</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>ThrmlZn:VentFlow</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Vent05_ExhaustFlowRate" type="hvac:ExhaustFlowRate" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Exhaust CFM</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>ThrmlZn:MinExhFlow Or ThrmlZn:ExhFlow</dtyp:CBECCnrXMLterm>
                                             <dtyp:termConditions>If ThrmlZn:MinExhFlow greater than 0 result = ThrmlZn:MinExhFlow  Else result = ThrmlZn:ExhFlow</dtyp:termConditions>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Vent06_ThermalZoneFloorArea" type="hvac:ThermalZoneFloorArea" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Conditioned Floor Area (sf)</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>ThrmlZn:FlrArea</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Vent07_MechVentControls" type="hvac:MechVentControls"  maxOccurs="unbounded">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">DCV or Occupant Sensor Controls, or Both</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ThrmlZn:HasDCV and ThrmlZn:HasOccSensorCtrl</dtyp:CBECCnrXMLterm>
                                                <dtyp:termConditions>If ThrmlZn:HasDCV == 1 result = DCV  Else If ThrmlZn:HasOccSensorCtrl == 1 result = OccupantSensors Else If If ThrmlZn:HasDCV == 1 And ThrmlZn:HasOccSensorCtrl == 1 result = OccSensorsDCV  Else If ThrmlZn:HasDCV != 1 And ThrmlZn:HasOccSensorCtrl != 1 result = NA</dtyp:termConditions>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Vent07_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">DCV or Occupant Sensor Controls, or Both</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ThrmlZn:HasDCV and ThrmlZn:HasOccSensorCtrl</dtyp:CBECCnrXMLterm>
                                                <dtyp:termConditions>If ThrmlZn:HasDCV != 1 And ThrmlZn:HasOccSensorCtrl != 1 result = NA</dtyp:termConditions>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_VentRes" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]H8. HIGH-RISE RESIDENTIAL DWELLING UNIT AND HOTEL/MOTEL VENTILATION [/d:b]</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">If project includes ventilation, this table is required If Info13_TopLevelOccupancy == HighRiseResidential Or HotelMotel Or ResidentialNR</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableVentRes" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="VentRes01_ThermalZoneName" type="hvac:ThermalZoneName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Zone Name</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>ThrmlZn:Name Or ThrmlZone:Name ThrmlZn:VentStdDesc + "ventilation"</dtyp:CBECCnrXMLterm>
                                             <dtyp:termConditions>If ThrmlZn:Type == Conditioned AND ThrmlZn:VentStd == Other result = ThrmlZone:Name ThrmlZn:VentStdDesc + "ventilation"  Else result = ThrmlZone:Name</dtyp:termConditions>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="VentRes02to07Subheading" minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Mechanical Ventilation</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="VentRes02_VentilationSpaceFunction" type="com:VentilationSpaceFunction"  maxOccurs="unbounded">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Ventilation Function</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="VentRes02_VentilationSpaceFunctions" type="com:VentilationSpaceFunctions" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Ventilation Function</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ThrmlZn:VentFuncList</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="VentRes03_HotelMotelGuestRoomCount" type="com:HotelMotelGuestRoomCount" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"># of hotel rooms</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>ThrmlZn:HotelMotelGuestRmCnt</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="VentRes04_BedroomCount" type="com:BedroomCount" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"># of bedrooms</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>ThrmlZn:ResLivingBdRmCnt</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="VentRes05_VentSystemOA" type="hvac:VentSystemOA" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Supply OA CFM</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>ThermalZn:VentFlow</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="VentRes06_ExhaustFlowRate" type="hvac:ExhaustFlowRate" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Exhaust CFM</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>ThermalZn:VentFlow Or ThrmlZn:MinExhFlow Or ThrmlZn:ExhFlow  See conditions</dtyp:CBECCnrXMLterm>
                                             <dtyp:termConditions>If ThrmlZn:HighRiseResVentType = "Balanced" or "HeatRecovery" then 
ThermalZn:VentFlow 
else if ThrmlZn:MinExhFlow GT 0 then 
ThrmlZn:MinExhFlow 
else ThrmlZn:ExhFlow


</dtyp:termConditions>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="VentRes07_ThermalZoneFloorArea" type="hvac:ThermalZoneFloorArea" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Conditioned Area (sf)</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>ThrmlZn:FlrArea</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="VentRes08_MechVentControls" type="hvac:MechVentControls"  maxOccurs="unbounded">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">DCV or Occupant Sensor Controls, or Both</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ThrmlZn:HasDCV and ThrmlZn:HasOccSensorCtrl</dtyp:CBECCnrXMLterm>
                                                <dtyp:termConditions>If ThrmlZn:HasDCV == 1 result = DCV  Else If ThrmlZn:HasOccSensorCtrl == 1 result = OccupantSensors Else If If ThrmlZn:HasDCV == 1 And ThrmlZn:HasOccSensorCtrl == 1 result = OccSensorsDCV  Else If ThrmlZn:HasDCV != 1 And ThrmlZn:HasOccSensorCtrl != 1 result = NA</dtyp:termConditions>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="VentRes08_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">DCV or Occupant Sensor Controls, or Both</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ThrmlZn:HasDCV and ThrmlZn:HasOccSensorCtrl</dtyp:CBECCnrXMLterm>
                                                <dtyp:termConditions>If ThrmlZn:HasDCV != 1 And ThrmlZn:HasOccSensorCtrl != 1 result = NA</dtyp:termConditions>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_Zone" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]H9. ZONAL SYSTEM AND TERMINAL UNIT SUMMARY[/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableZone" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Zone01_TerminalUnitName" type="hvac:TerminalUnitName" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">System ID</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>TrmlUnit</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Zone01_ZoneSystemName" type="hvac:ZoneSystemName" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">System ID</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ZnSys:Desc</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="Zone02_ThermalZoneName" type="hvac:ThermalZoneName"  minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Zone Name</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>TrmlUnit:ZnServedRef:Desc Or ThrmlZn:Desc If  ThrmlZn:PriAirCondgSysRef OR ExhSysRef  OR VentSysRef  == ZnSys:Desc in Zone01</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Zone03_TerminalUnitType" type="hvac:TerminalUnitType" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">System Type</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>TrmlUnit:Type</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Zone03_ZoneSystemType" type="hvac:ZoneSystemType" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">System Type</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ZnSys:Type</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Zone04_TerminalUnitCount" type="hvac:TerminalUnitCount" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Qty</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>TrmlUnit:SysCnt</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Zone04_ZoneSysCount" type="hvac:ZoneSysCount" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Qty</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ZnSys:Cnt</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="Zone05to06ColLabel" minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Rated Capacity (kBtuh)</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Zone05_HeatingRatedCapacity" type="hvac:HeatingRatedCapacity" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Heating</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ZnSys:CoilHtg:CapTotGrossRtd Or CapTotNetRtd Or ThrmlUnit:CoilHtg:CapTotNetRtd ??</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
