                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">DESIGN WATTS</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="TableIntLtgSysOrnamental" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"></xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                                <xsd:complexType>
                                                   <xsd:sequence>
                                                      <xsd:element name="TailoredOrnamental06_InteriorLightingSystemName" type="lit:InteriorLightingSystemName" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Interior Lighting System</xsd:documentation>
