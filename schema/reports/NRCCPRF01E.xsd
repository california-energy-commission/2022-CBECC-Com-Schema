                                                                              <xsd:documentation source="FieldText">Watts per Luminaire</xsd:documentation>
                                                                              <xsd:appinfo source="CBECCnrXML">
                                                                                 <dtyp:CBECCnrXMLterm>Spc:IntLtgSys:LumPwr[]</dtyp:CBECCnrXMLterm>
                                                                              </xsd:appinfo>
                                                                           </xsd:annotation>
                                                                        </xsd:element>
                                                                        <xsd:element name="TailoredFloor11_LuminaireCountByType" type="lit:LuminaireCountByType" >
                                                                           <xsd:annotation>
                                                                              <xsd:documentation source="FieldText"># of Luminaires</xsd:documentation>
                                                                              <xsd:appinfo source="CBECCnrXML">
                                                                                 <dtyp:CBECCnrXMLterm>Spc:IntLtgSys:LumCnt[]</dtyp:CBECCnrXMLterm>
                                                                              </xsd:appinfo>
                                                                           </xsd:annotation>
                                                                        </xsd:element>
                                                                     </xsd:sequence>
                                                                  </xsd:complexType>
                                                               </xsd:element>
                                                            </xsd:sequence>
                                                         </xsd:complexType>
                                                      </xsd:element>
                                                   </xsd:sequence>
                                                </xsd:complexType>
                                             </xsd:element>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="TailoredFloor12_TailoredPowerInArea" type="comp:TailoredPowerInArea" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Total Design Watts</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Spc:TMCmbdFlrDispTaskDsgn</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="TailoredFloor13_PowerAllowedFloorDisplayOrTask" type="comp:PowerAllowedFloorDisplayOrTask" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Allowed Watts (smaller of allotted vs design watts)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Result = Smallest (TailoredFloor12_TailoredPowerInArea, TailoredFloor05_ TailoredPowerCalculatedForArea )</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Spc:TMCmbdFlrDispTaskAllow</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_TailoredOrnamental" minOccurs="0">
