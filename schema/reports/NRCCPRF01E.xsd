                                    <xsd:element name="Fen04_AssemblyContext" type="com:AssemblyContext" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Assembly Method</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Proj:FenCons:AssmContext</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Fen05_FenestrationAssemblyArea" type="env:FenestrationAssemblyArea" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Area (ft[d:sup]2[/d:sup])</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Proj:FenCons:AreaNew or Proj:FenCons:AreaExisting or Proj:FenCons:AreaAltered</dtyp:CBECCnrXMLterm>
                                             <dtyp:termConditions>If Proj:FenCons:Status == New 
result = Proj:FenCons:AreaNew
Else If Proj:FenCons:Status == Existing 
result = Proj:FenCons:AreaExisting
Else If Proj:FenCons:Status == Altered 
result = Proj:FenCons:AreaAltered
End If
</dtyp:termConditions>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Fen06_FenestrationAssemblyUFactor" type="env:FenestrationAssemblyUFactor" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Overall U-factor</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Use this choice If Fen03_FenestrationCertificationMethod != COGEquations
End If</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>Proj:FenCons:Ufactor</dtyp:CBECCnrXMLterm>
                                                <dtyp:termConditions>If Proj:FenCons:CertificationMthd != COGEquations
result = Proj:FenCons:Ufactor</dtyp:termConditions>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Fen06_UFactorCenterOfGlass" type="com:UFactorCenterOfGlass" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Overall U-factor</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Use this choice If Fen03_FenestrationCertificationMethod == COGEquations
End If</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>Proj:FenCons:UfactorCOG</dtyp:CBECCnrXMLterm>
                                                <dtyp:termConditions>If Proj:FenCons:CertificationMthd == COGEquations
result = Proj:FenCons:UfactorCOG</dtyp:termConditions>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Fen07_FenestrationAssemblySHGC" type="env:FenestrationAssemblySHGC" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Overall SHGC</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Use this choice If Fen03_FenestrationCertificationMethod != COGEquations
End If</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>Proj:FenCons:SHGC</dtyp:CBECCnrXMLterm>
                                                <dtyp:termConditions>If Proj:FenCons:CertificationMthd != COGEquations
result = Proj:FenCons:SHGC</dtyp:termConditions>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Fen07_SHGC_CenterOfGlass" type="com:SHGC_CenterOfGlass" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Overall SHGC</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Use this choice If Fen03_FenestrationCertificationMethod == COGEquations
End If</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>Proj:FenCons:SHGCCOG</dtyp:CBECCnrXMLterm>
                                                <dtyp:termConditions>If Proj:FenCons:CertificationMthd == COGEquations
result = Proj:FenCons:SHGCCOG</dtyp:termConditions>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Fen08_FenestrationAssemblyVT" type="env:FenestrationAssemblyVT" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Overall VT</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Use this choice If Fen03_FenestrationCertificationMethod != COGEquations
End If</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>Proj:FenCons:VT</dtyp:CBECCnrXMLterm>
                                                <dtyp:termConditions>If Proj:FenCons:CertificationMthd != COGEquations
result = Proj:FenCons:VT</dtyp:termConditions>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Fen08_VT_CenterOfGlass" type="com:VT_CenterOfGlass" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Overall VT</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Use this choice If Fen03_FenestrationCertificationMethod == COGEquations
End If</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>Proj:FenCons:VTCOG</dtyp:CBECCnrXMLterm>
                                                <dtyp:termConditions>If Proj:FenCons:CertificationMthd == COGEquations
result = Proj:FenCons:VTCOG</dtyp:termConditions>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="Fen09_Status" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Status[d:sup]2[/d:sup]</xsd:documentation>
                                          <xsd:appinfo source="displayterm">
                                             <dtyp:displayterm value="ExistingAlteration">A</dtyp:displayterm>
                                             <dtyp:displayterm value="ExistingUnchanged">E</dtyp:displayterm>
                                             <dtyp:displayterm value="New">N</dtyp:displayterm>
                                          </xsd:appinfo>
                                          
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Proj:FenCons:Status</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="com:Status">
                                             <xsd:enumeration value="ExistingAlteration"/>
                                             <xsd:enumeration value="ExistingUnchanged"/>
                                             <xsd:enumeration value="New"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="FenFootNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]1[/d:sup] Newly installed fenestration shall have a certified NFRC Label Certificate or use the CEC default tables found in Table 110.6-A and Table 110.6-B. Center of Glass (COG) values are for the glass-only, determined by the manufacturer, and are shown for ease of verification. Site-built fenestration values are calculated per Nonresidential Appendix NA6 and are used in the analysis.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="FenFootNote2" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:i][d:sup]2[/d:sup] Status: N - New, A - Altered, E - Existing[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_OverHang" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]G6. OVERHANG DETAILS [/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableOverHang" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="OverHang01_FenestrationName" type="env:FenestrationName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Fenestration Tag/ ID</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Proj:Bldg:Story;Spc:ExtWall:Win:Name</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="OverHang02_CardinalOrientation" type="com:CardinalOrientation" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Orientation</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Proj:Bldg:Story;Spc:ExtWall:Win:OrientationRpt</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="OverHang03_OverhangDepth" type="com:OverhangDepth" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Depth (ft)</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Proj:Bldg:Story;Spc:ExtWall:Win:OverhangDepth</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="OverHang04_OverhangHeight" type="env:OverhangHeight" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Height from Bottom of Sill to Overhang (ft)</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Proj:Bldg:Story;Spc:ExtWall:Win:Y + Proj:Bldg:Story;Spc:ExtWall:Win:Hgt + Proj:Bldg:Story;Spc:ExtWall:Win:OverhangDistance</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="OverHang05_OverhangRightExtent" type="env:OverhangRightExtent" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Right Extent (ft)</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Proj:Bldg:Story;Spc:ExtWall:Win:OverhangRightExtension</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="OverHang06_OverhangLeftExtent" type="env:OverhangLeftExtent" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Left Extent (ft)</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Proj:Bldg:Story;Spc:ExtWall:Win:OverhangLeftExtension</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_Fins" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]G7. FIN DETAILS [/d:b]</xsd:documentation>
               <xsd:appinfo source="CBECCnrXML">
                  <dtyp:CBECCnrXMLterm>This section is required if any condition is true</dtyp:CBECCnrXMLterm>
                  <dtyp:termConditions>Proj:Bldg:Story;Spc:ExtWall:Win:RightFinDepth GT 0
Proj:Bldg:Story;Spc:ExtWall:Win:LeftFinDepth GT 0</dtyp:termConditions>
               </xsd:appinfo>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableFins" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Fins01_FenestrationName" type="env:FenestrationName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Fenestration Tag/ ID</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Proj:Bldg:Story;Spc:ExtWall:Win:Name</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Fins0203Subheading" minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Left Fin</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Fins02_FinDepth" type="env:FinDepth"  minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Depth (ft)</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Proj:Bldg:Story;Spc:ExtWall:Win:LeftFinDepth</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Fins03_FinDistanceLeft" type="env:FinDistanceLeft"  minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Distance from Window (ft)</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Proj:Bldg:Story;Spc:ExtWall:Win:LeftFinDistance</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Fins0405Subheading" minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Right Fin</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Fins04_FinDepth" type="env:FinDepth"  minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Depth (ft)</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Proj:Bldg:Story;Spc:ExtWall:Win:RightFinDepth</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Fins05_FinDistanceRight" type="env:FinDistanceRight"  minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Distance from Window (ft)</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Proj:Bldg:Story;Spc:ExtWall:Win:RightFinDistance</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_HvacDry" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]H1. DRY SYSTEM EQUIPMENT (furnaces, air handling units, heat pumps, VRF, economizers etc.) [/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableHvacDry" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="HvacDry01_HvacSystemName" type="hvac:HvacSystemName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Equipment Name</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">This is a string that identifies either an  Air System, Zone System or VRF System</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>AirSys:Nameor ZnSys:Name or VRFSys:Name</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="HvacDry02_AirSystemType" type="hvac:AirSystemType" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Equipment Type</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>AirSys:Type</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="HvacDry02_ZoneSystemType" type="hvac:ZoneSystemType" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Equipment Type</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ZnSys:Type</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="HvacDry02a_AirSubType" type="hvac:AirSubType"  minOccurs="0">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Equipment Type</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If there is a value for this field append the display term in parenthesis ( ) after the HvacDry02 value</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>AirSys:SubType</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="HvacDry02a_ZoneSubType" type="hvac:ZoneSubType"  minOccurs="0">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Equipment Type</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If there is a value for this field append the display term in parenthesis ( ) after the HvacDry02 value</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ZnSys:SubType</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="HvacDry03_HvacEquipmentCount" type="hvac:HvacEquipmentCount" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Qty</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>AirSys:Cnt Or ZnSys:Cnt  Or VRFSys:Cnt</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="HvacDryCol04to07Label" minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">[d:b]Heating [/d:b]</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="HvacDry04_HeatingDesignCapacity" type="hvac:HeatingDesignCapacity" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Total Heating Output [d:line1/] (kBtu/h)</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>AirSys:HtgCap/1000 Or ZnSys:HtgCap/1000 Or VRFSys:HtgCapNetRtd/1000</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="HvacDry05_HeatingSupplementalCapacity" type="hvac:HeatingSupplementalCapacity" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Supp Heat Output [d:line1/] (kBtu/h)</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>AirSys:SuppHtgCap/1000 Or ZnSys:SuppHtgCap/1000</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="HvacDry05_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Supp Heat Output [d:line1/] (kBtu/h)</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="HvacDry06a_EfficiencyType" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Efficiency Units</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>AirSys:HtgEffUnitRpt Or ZnSys:HtgEffUnitRpt Or VRFSys:HtgEffUnitRpt</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="hvac:EfficiencyType">
                                                <xsd:enumeration value="AFUE"/>
                                                <xsd:enumeration value="CombustionEfficiency"/>
                                                <xsd:enumeration value="COP"/>
                                                <xsd:enumeration value="HSPF"/>
                                                <xsd:enumeration value="ThermalEfficiency"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                       <xsd:element name="HvacDry06a_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Efficiency Units</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="HvacDry06b_EfficiencyAFUE" type="hvac:EfficiencyAFUE" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Efficiency</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>AirSys:HtgEffRpt Or ZnSys:HtgEffRpt Or VRFSys:HtgEffRpt</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="HvacDry06b_EfficiencyCOP" type="hvac:EfficiencyCOP" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Efficiency</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>AirSys:HtgEffRpt Or ZnSys:HtgEffRpt Or VRFSys:HtgEffRpt</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="HvacDry06b_EfficiencyHSPF" type="hvac:EfficiencyHSPF" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Efficiency</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>AirSys:HtgEffRpt Or ZnSys:HtgEffRpt Or VRFSys:HtgEffRpt</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="HvacDry06b_ThermalEfficiency" type="hvac:ThermalEfficiency" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Efficiency</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>AirSys:HtgEffRpt Or ZnSys:HtgEffRpt Or VRFSys:HtgEffRpt</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="HvacDry06b_EfficiencyNA" type="comp:EfficiencyNA" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Efficiency</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
