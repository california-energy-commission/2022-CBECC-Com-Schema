         <xsd:element name="Section_CRRC" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]G2. CRRC ROOFING PRODUCT SUMMARY  [/d:b]</xsd:documentation>
               <xsd:appinfo source="CBECCnrXML">
                  <dtyp:CBECCnrXMLterm>This section is required if all conditions are true</dtyp:CBECCnrXMLterm>
                  <dtyp:termConditions>p:Proj:CompPathEnv == Performance And 
p:ConsAssm:CompatibleSufrType == Roof And
p:ConsAssm:HasCRRCProdID == 1</dtyp:termConditions>
               </xsd:appinfo>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableCRRC" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="CRRC01_RoofAssemblyName" type="env:RoofAssemblyName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Assembly Name</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="CRRC02_RoofSlope" type="env:RoofSlope" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Roof Pitch</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="CRRC03_SolarReflectanceAged" type="com:SolarReflectanceAged" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Aged Solar Reflectance</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="CRRC04_ThermalEmittance" type="com:ThermalEmittance" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Thermal Emittance</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="CRRC05_SolarReflectanceIndex" type="env:SolarReflectanceIndex" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">SRI</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="CRRC05_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">SRI</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_Opaq" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]G3. OPAQUE SURFACE ASSEMBLY SUMMARY  [/d:b]</xsd:documentation>
               <xsd:appinfo source="CBECCnrXML">
                  <dtyp:CBECCnrXMLterm>This section is required if the condition is true</dtyp:CBECCnrXMLterm>
                  <dtyp:termConditions>p:Proj:CompPathEnv == Performance</dtyp:termConditions>
               </xsd:appinfo>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableOpaq" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Opaq01_ConstructionAssemblyNameString" type="env:ConstructionAssemblyNameString" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Construction Assembly Name</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Proj:ConsAssm:Name</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Opaq02_CompatibleSurfaceType" type="env:CompatibleSurfaceType" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Surface Type</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Proj:ConsAssm:CompatibleSurfType</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Opaq03_SurfaceArea" type="dtyp:SurfaceArea" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Area (ft[d:sup]2[/d:sup])</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Proj:ConsAssm:AreaNew or Proj:ConsAssm:AreaExisting or Proj:ConsAssm:AreaAltered</dtyp:CBECCnrXMLterm>
                                             <dtyp:termConditions>If Proj:ConsAssm:Status == New result = Proj:ConsAssm:AreaNew Else If Proj:ConsAssm:Status == Existing result = Proj:ConsAssm:AreaExisting Else If Proj:ConsAssm:Status == Altered result = Proj:ConsAssm:AreaAltered End If</dtyp:termConditions>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Opaq04_PartitionFramingMaterial" type="env:PartitionFramingMaterial" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Framing Type</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>Proj:ConsAssm:FrmMat</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Opaq04_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Framing Type</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Opaq05_CavityInsulation" type="env:CavityInsulation" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Cavity R-Value</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>Proj:ConsAssm:CavityInsVal  If Proj:ConsAssm:CavityInsVal  GT  0</dtyp:CBECCnrXMLterm>
                                                <dtyp:termConditions>If Proj:ConsAssm:CavityInsVal  GT  0 result = Proj:ConsAssm:CavityInsVal Else result = NotApplicable stored in NotApplicableMessage End If
</dtyp:termConditions>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Opaq05_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Cavity R-Value</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>NotApplicable If Proj:ConsAssm:CavityInsVal  == 0</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Opaq06_InsulationRValue" type="env:InsulationRValue" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Continuous R-Value</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>Proj:ConsAssm:InsCheckSum If Proj:ConsAssm:InsCheckSum GT  0</dtyp:CBECCnrXMLterm>
                                                <dtyp:termConditions>If Proj:ConsAssm:InsCheckSum GT  0 result = Proj:ConsAssm:InsCheckSum 
Else result = NotApplicable stored in NotApplicableMessage
End If</dtyp:termConditions>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Opaq06_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Continuous R-Value</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>NotApplicable If Proj:ConsAssm:InsCheckSum  == 0</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="Opaq07_ConstructionSpecificationMethod" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Units</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>ConsAssm:SpecMthd</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="env:ConstructionSpecificationMethod">
                                             <xsd:enumeration value="CFactor"/>
                                             <xsd:enumeration value="FFactor"/>
                                             <xsd:enumeration value="UFactor"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Opaq08_UFactor" type="com:UFactor" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Value</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ConsAssm:UFactor</dtyp:CBECCnrXMLterm>
                                                <dtyp:termConditions>If ConsAssm:SpecMthd == Layers result = ConsAssm:UFactor</dtyp:termConditions>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Opaq08_FFactor" type="com:FFactor" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Value</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ConsAssm:FFactor</dtyp:CBECCnrXMLterm>
                                                <dtyp:termConditions>If ConsAssm:SpecMthd == FFactor result = ConsAssm:FFactor
End If</dtyp:termConditions>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Opaq08_CFactor" type="com:CFactor" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Value</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ConsAssm:CFactor</dtyp:CBECCnrXMLterm>
                                                <dtyp:termConditions>If ConsAssm:SpecMthd == CFactor result = ConsAssm:CFactor
End If</dtyp:termConditions>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="Opaq09" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Description of Assembly Layers</xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:complexType>
                                          <xsd:choice >
                                             <xsd:element name="Opaq09Slab" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText"></xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:complexType>
                                                   <xsd:sequence>
                                                      <xsd:element name="Opaq09slab_SlabType" type="env:SlabType" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Slab Type =</xsd:documentation>
                                                            <xsd:appinfo source="CBECCnrXML">
                                                               <dtyp:CBECCnrXMLterm>Proj:ConsAssm:SlabType</dtyp:CBECCnrXMLterm>
                                                            </xsd:appinfo>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="Opaq09slab_SlabInsulationOrientation" type="env:SlabInsulationOrientation" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Insulation Orientation =</xsd:documentation>
                                                            <xsd:appinfo source="CBECCnrXML">
                                                               <dtyp:CBECCnrXMLterm>Proj:ConsAssm:SlabInsOrientation</dtyp:CBECCnrXMLterm>
                                                            </xsd:appinfo>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="Opaq09slab_SlabInsulationThermalResistance" type="env:SlabInsulationThermalResistance" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Insulation R-Value =</xsd:documentation>
                                                            <xsd:appinfo source="CBECCnrXML">
                                                               <dtyp:CBECCnrXMLterm>Proj:ConsAssm:SlabInsThrmlR</dtyp:CBECCnrXMLterm>
                                                            </xsd:appinfo>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                   </xsd:sequence>
                                                </xsd:complexType>
                                             </xsd:element>
                                             <xsd:element name="Opaq09Layers" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText"></xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:complexType>
                                                   <xsd:sequence>
                                                      <xsd:choice maxOccurs="1">
                                                         <xsd:element name="Opaq09layer1_CodeItem" type="env:CodeItem" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText"></xsd:documentation>
                                                               <xsd:appinfo source="CBECCnrXML">
                                                                  <dtyp:CBECCnrXMLterm>Proj:ConsAssm:MatRef</dtyp:CBECCnrXMLterm>
                                                                  <dtyp:termConditions>Proj:ConsAssm:MatRef index = 0</dtyp:termConditions>
                                                               </xsd:appinfo>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                         <xsd:element name="Opaq09layer1_CodeItemGlazing" type="env:CodeItemGlazing" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText"></xsd:documentation>
                                                               <xsd:appinfo source="CBECCnrXML">
                                                                  <dtyp:CBECCnrXMLterm>Proj:ConsAssm:MatRef</dtyp:CBECCnrXMLterm>
                                                                  <dtyp:termConditions>Proj:ConsAssm:MatRef index = 0</dtyp:termConditions>
                                                               </xsd:appinfo>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                         <xsd:element name="Opaq09layer1_MaterialName" type="env:MaterialName" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText"></xsd:documentation>
                                                               <xsd:appinfo source="CBECCnrXML">
                                                                  <dtyp:CBECCnrXMLterm>Proj:ConsAssm:MatRef</dtyp:CBECCnrXMLterm>
                                                                  <dtyp:termConditions>Proj:ConsAssm:MatRef index = 0  If value does not match enums in CodeItem choice Or CodeItemGlazing choice, use this choice for string value</dtyp:termConditions>
                                                               </xsd:appinfo>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                      </xsd:choice>
                                                      <xsd:choice maxOccurs="1">
                                                         <xsd:element name="Opaq09layer2_CodeItem" type="env:CodeItem"  minOccurs="0">
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText"></xsd:documentation>
                                                               <xsd:appinfo source="CBECCnrXML">
                                                                  <dtyp:CBECCnrXMLterm>Proj:ConsAssm:MatRef</dtyp:CBECCnrXMLterm>
                                                                  <dtyp:termConditions>Proj:ConsAssm:MatRef index = 1
</dtyp:termConditions>
                                                               </xsd:appinfo>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                         <xsd:element name="Opaq09layer2_CodeItemGlazing" type="env:CodeItemGlazing"  minOccurs="0">
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText"></xsd:documentation>
                                                               <xsd:appinfo source="CBECCnrXML">
                                                                  <dtyp:CBECCnrXMLterm>Proj:ConsAssm:MatRef</dtyp:CBECCnrXMLterm>
                                                                  <dtyp:termConditions>Proj:ConsAssm:MatRef index = 1
</dtyp:termConditions>
                                                               </xsd:appinfo>
