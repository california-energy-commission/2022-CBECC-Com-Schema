                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText"></xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Fen03_FenestrationCertificationMethod != NFRCRated possible values are MetalFraming, MetalFramingWithThermalBreak, NonMetalFraming</xsd:documentation>
