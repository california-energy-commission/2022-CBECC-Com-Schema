                                                                              <xsd:documentation source="FieldText">Luminaire Name or Item Tag</xsd:documentation>
                                                                              <xsd:appinfo source="CBECCnrXML">
                                                                                 <dtyp:CBECCnrXMLterm>Spc:IntLtgSys:LumRef[]</dtyp:CBECCnrXMLterm>
                                                                              </xsd:appinfo>
                                                                           </xsd:annotation>
                                                                        </xsd:element>
                                                                        <xsd:element name="TailoredFloor10_LuminairePower" type="lit:LuminairePower" >
