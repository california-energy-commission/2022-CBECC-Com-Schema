         <xsd:element name="Section_WaterCompact" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]I7.  WATER HEATING - COMPACT DISTRIBUTION [/d:b]</xsd:documentation>
               <xsd:appinfo source="CBECCnrXML">
                  <dtyp:CBECCnrXMLterm>This section is required If Spc:ResSpcDHWFeatures:CompactDistErrorID = 0  AND Spc:ResSpcDHWFeatures:CompactDistIdx GT 0</dtyp:CBECCnrXMLterm>
               </xsd:appinfo>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableWaterCompact" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="WaterCompact01_SpaceName" type="com:SpaceName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Name</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Spc:Name</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="WaterCompact02_DwellingUnitType" type="com:DwellingUnitType" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Dwelling Unit Type</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>DwellingUnitBedroomCnt</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="WaterCompact03_WaterHeatingSystemName" type="hvac:WaterHeatingSystemName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Water Heating System Name</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Spc:ResDHWSysRef</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="WaterCompact04_WaterHeaterToBath" type="comp:WaterHeaterToBath" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Master Bath distance of furthest fixture to Water Heater (ft)</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>Spc:ResSpcDHWFeatures:MasterBathFxtrDistance</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="WaterCompact04_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Master Bath distance of furthest fixture to Water Heater (ft)</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="WaterCompact05_WaterHeaterToKitchen" type="comp:WaterHeaterToKitchen" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Kitchen distance of furthest fixture to Water Heater (ft)</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>Spc:ResSpcDHWFeatures:KitFxtrDistance</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="WaterCompact05_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Kitchen distance of furthest fixture to Water Heater (ft)</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="WaterCompact06_WaterHeaterToFurthest3rdFixture" type="comp:WaterHeaterToFurthest3rdFixture" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Furthest Third furthest fixture to Water Heater (ft)</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>Spc:ResSpcDHWFeatures:ThirdRoomFxtrDistance</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="WaterCompact06_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Furthest Third furthest fixture to Water Heater (ft)</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="WaterCompact07_CompactDistributionVerificationRequired" type="comp:CompactDistributionVerificationRequired" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">HERS Verification</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Spc:ResSpcDHWFeatures:CompactDistIdx = 2 result = true; else false</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_WaterDWHR" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">I8. WATER HEATING - DRAIN WATER HEAT RECOVERY</xsd:documentation>
               <xsd:appinfo source="CBECCnrXML">
                  <dtyp:CBECCnrXMLterm>This section is required If Spc:NumResDWHRSysRefs GT 0 AND Spc:TotResDWHRSysShowers GT 0</dtyp:CBECCnrXMLterm>
               </xsd:appinfo>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableDWHR" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="WaterDWHR01_SpaceName" type="com:SpaceName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Name</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">For display space name should be concatenated with  dwelling unit type that defines bedroom count like this: 
WaterDWHR01_SpaceName + " - " + WaterDWHR01a_DwellingUnitType

This string is displayed on each row that has a different DWHR for this space</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Spc:ResSpcDHWFeatures:ParentSpcNameRp</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="WaterDWHR01a_DwellingUnitType" type="com:DwellingUnitType" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Dwelling Unit Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">For display space name should be concatenated with  dwelling unit type that defines bedroom count like this: 
WaterDWHR01_SpaceName + " - " + WaterDWHR01a_DwellingUnitType</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Spc:ResSpcDHWFeatures:DwellingUnitBedroomCnt</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="WaterDWHR02_WaterHeatingSystemName" type="hvac:WaterHeatingSystemName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">DHW System and DWHR Names</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">For display the DHW system name should be concatenated with a DWHR name  like this: 
WaterDWHR02_WaterHeatingSystemName + " - " + WaterDWHR02a_DWHR_Name</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Spc:ResSpcDHWFeatures:ParentResDHWSysRefRpt</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="WaterDWHR02a_DWHR_Name" type="comp:DWHR_Name" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">DHW System and DWHR Names</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">For display the DHW system name should be concatenated with a DWHR name  like this: 
WaterDWHR02_WaterHeatingSystemName + " - " + WaterDWHR02a_DWHR_Name</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Spc:ResSpcDHWFeatures:ResDWHRSysRef[]</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="WaterDWHR03_DWHR_Configuration" type="comp:DWHR_Configuration" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Installation Configuration</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Spc:ResSpcDHWFeatures:ResDWHRSysConfig[]</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="WaterDWHR04_DWHR_DrainCount" type="comp:DWHR_DrainCount" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Shower Drains</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Spc:ResSpcDHWFeatures:ResDWHRSysShowers[]</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="WaterDWHR05_DWHR_HERS_VerificationRequired" type="comp:DWHR_HERS_VerificationRequired" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">HERS Verification</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>"Required" else "n/a"</dtyp:CBECCnrXMLterm>
                                             <dtyp:termConditions>If Spc:ResSpcDHWFeatures:NumResDWHRSysDevs = 1 then "Required" else "n/a"</dtyp:termConditions>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_ProcGarage" minOccurs="0">
