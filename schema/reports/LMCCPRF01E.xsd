            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableFins" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Fins01_FenestrationName" type="env:FenestrationName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Fenestration Tag/ ID</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Proj:Bldg:Story;Spc:ExtWall:Win:Name</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Fins0203Subheading" minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Left Fin</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Fins02_FinDepth" type="env:FinDepth"  minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Depth (ft)</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Proj:Bldg:Story;Spc:ExtWall:Win:LeftFinDepth</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Fins03_FinDistanceLeft" type="env:FinDistanceLeft"  minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Distance Left from Window (ft)</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Proj:Bldg:Story;Spc:ExtWall:Win:LeftFinDistance</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Fins0405Subheading" minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Right Fin</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Fins04_FinDepth" type="env:FinDepth"  minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Depth (ft)</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Proj:Bldg:Story;Spc:ExtWall:Win:RightFinDepth</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Fins05_FinDistanceRight" type="env:FinDistanceRight"  minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Distance Right from Window (ft)</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Proj:Bldg:Story;Spc:ExtWall:Win:RightFinDistance</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_HvacDry" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]H1. DRY SYSTEM EQUIPMENT (furnaces, air handling units, heat pumps, VRF, economizers etc.) [/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableHvacDry" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="HvacDry01_HvacSystemName" type="comp:HvacSystemName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Equipment Name</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">This is a string that identifies either an  Air System, Zone System or VRF System</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>AirSys:Packaged ZnSys:Name or VRFSys:Name</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="HvacDry02_AirSystemType" type="hvac:AirSystemType" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Equipment Type</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>AirSys:Type</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="HvacDry02_ZoneSystemType" type="hvac:ZoneSystemType" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Equipment Type</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ZnSys:Type</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="HvacDry02a_AirSubType" type="hvac:AirSubType"  minOccurs="0">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText"></xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If there is a value for this field append the display term in parenthesis ( ) after the HvacDry02 value</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>AirSys:SubType</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="HvacDry02a_ZoneSubType" type="hvac:ZoneSubType"  minOccurs="0">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText"></xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If there is a value for this field append the display term in parenthesis ( ) after the HvacDry02 value</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ZnSys:SubType</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="HvacDry03_HvacEquipmentCount" type="hvac:HvacEquipmentCount" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Qty</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>AirSys:Cnt Or ZnSys:Cnt  Or VRFSys:Cnt</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="HvacDryCol04to07Label" minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">[d:b]Heating [/d:b]</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="HvacDry04_HeatingDesignCapacity" type="hvac:HeatingDesignCapacity" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Total Heating Output [d:line1/] (kBtu/h)</xsd:documentation>
