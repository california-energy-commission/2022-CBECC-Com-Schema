                  </xsd:element>
                  <xsd:element name="ProjPresc02b" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:i]LMCC-LTO-01E is required[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="ProjPresc03a" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Sign Lighting [d:t24refnolink/]140.8 &amp; [d:t24refnolink/]170.2(e)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="ProjPresc03b" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:i]LMCC-LTS-01E is required[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="ProjMMSubheading" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:i]Building Components Complying with Mandatory Measures [/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="ProjMMNote" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]Electrical power systems, commissioning, solar ready, elevator and escalator requirements are mandatory and should be documented on the LMCC form listed if applicable (i.e. compliance will not be shown on the LMCC-PRF-E.)[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="ProjMM01a" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Electrical Power Distribution [d:t24refnolink/]110.11</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="ProjMM01b" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:i]LMCC-ELC-01E is required[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="ProjMM02a" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Commissioning [d:t24refnolink/]120.8</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="ProjMM02b" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:i]LMCC-CXR-01E is required[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="ProjMM03a" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Solar and Battery  [d:t24refnolink/]110.10</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="ProjMM03b" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:i]LMCC-SAB-01E is required[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_DwellInfo" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]F3. DWELLING UNIT INFORMATION[/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableDwellingUnits" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="DwellInfo01_ResidentialDwellingUnitName" type="com:ResidentialDwellingUnitName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Dwelling Unit Name</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="DwellInfo02_DwellingUnitType" type="com:DwellingUnitType" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Dwelling Unit Type</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="DwellInfo02_ResidentialDwellingUnitDescriptor" type="com:ResidentialDwellingUnitDescriptor" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Dwelling Unit Type</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="DwellInfo03_ThermalZoneName" type="com:ThermalZoneName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Zone</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="DwellInfo04_ZoneGroupMultiplier" type="hvac:ZoneGroupMultiplier" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Zone Group Multiplier</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="DwellInfo04a_ImprovementStatus" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:simpleType>
					  <xsd:restriction base="com:ImprovementStatus">
					     <xsd:enumeration value="Existing"/>
					     <xsd:enumeration value="New"/>
					  </xsd:restriction>
				       </xsd:simpleType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
