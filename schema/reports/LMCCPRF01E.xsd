                                             <dtyp:CBECCnrXMLterm>AirSys:HtgCap/1000 Or ZnSys:HtgCap/1000 Or VRFSys:HtgCapNetRtd/1000</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="HvacDry05_HeatingSupplementalCapacity" type="hvac:HeatingSupplementalCapacity" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Supp Heat Output [d:line1/] (kBtu/h)</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>AirSys:SuppHtgCap/1000 Or ZnSys:SuppHtgCap/1000</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="HvacDry05_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Supp Heat Output [d:line1/] (kBtu/h)</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="HvacDry06a_EfficiencyType" maxOccurs="2">
