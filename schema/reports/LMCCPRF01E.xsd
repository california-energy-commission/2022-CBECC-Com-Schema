                        </xsd:restriction>
                     </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="Info11_WeatherFile" type="com:WeatherFile" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Weather File</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Info12_ProjectScopeNR" type="comp:ProjectScopeNR"  maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Project Scope</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Info13_DwellingUnitsCount" type="com:DwellingUnitsCount" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Number of Dwelling Units</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Info14_TotalConditionedFloorArea" type="com:TotalConditionedFloorArea" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Total Conditioned Floor Area in Scope (ft[d:sup]2[/d:sup])</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Info15_HotelMotelGuestRoomCount" type="com:HotelMotelGuestRoomCount" >
