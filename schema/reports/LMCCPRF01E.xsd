                                             <xsd:documentation source="FieldText">Supply Airflow CFM</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="VentRes04_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Supply Airflow CFM</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="VentRes05_FanPowerPerFlow" type="hvac:FanPowerPerFlow" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Supply Fan Efficacy W/CFM</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="VentRes05_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Supply Fan Efficacy W/CFM</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="VentRes06_ExhaustFlowRate" type="hvac:ExhaustFlowRate" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Exhaust CFM</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="VentRes06_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Exhaust CFM</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="VentRes07_FanPowerPerFlow" type="hvac:FanPowerPerFlow" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Exhaust Fan Efficacy W/CFM</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="VentRes07_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Exhaust Fan Efficacy W/CFM</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="VentRes0813Label" minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Individual Fan (if applicable)</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="VentRes08_VentilationSystemType"  maxOccurs="unbounded">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">IAQ Fan Type</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="hvac:VentilationSystemType">
                                                <xsd:enumeration value="Balanced"/>
                                                <xsd:enumeration value="BalancedERV"/>
                                                <xsd:enumeration value="BalancedHRV"/>
                                                <xsd:enumeration value="ExhaustVent"/>
                                                <xsd:enumeration value="SupplyVent"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                       <xsd:element name="VentRes08_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">IAQ Fan Type</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="VentRes09_FanCount" type="hvac:FanCount"  maxOccurs="unbounded">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Count</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="VentRes09_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Count</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="VentRes10_FanFlowCapacity" type="hvac:FanFlowCapacity"  maxOccurs="unbounded">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Airflow CFM</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="VentRes10_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Airflow CFM</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="VentRes11_FanPowerPerFlow" type="hvac:FanPowerPerFlow"  maxOccurs="unbounded">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Fan Efficacy W/CFM</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="VentRes11_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Fan Efficacy W/CFM</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="VentRes12_SensibleRecoveryEfficiency" type="hvac:SensibleRecoveryEfficiency"  maxOccurs="unbounded">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Recovery Efficiency SRE</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="VentRes12_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Recovery Efficiency SRE</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="VentRes13_AdjustedSensibleRecoveryEfficiency" type="hvac:AdjustedSensibleRecoveryEfficiency"  maxOccurs="unbounded">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Recovery Efficiency ASRE</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="VentRes13_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Recovery Efficiency ASRE</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_Zone" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]H11. ZONAL SYSTEM AND TERMINAL UNIT SUMMARY[/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableZone" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Zone01_TerminalUnitName" type="hvac:TerminalUnitName" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">System ID</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>TrmlUnit</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Zone01_ZoneSystemName" type="hvac:ZoneSystemName" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">System ID</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ZnSys:Desc</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="Zone02_ThermalZoneName" type="hvac:ThermalZoneName"  minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Zone Name</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>TrmlUnit:ZnServedRef:Desc Or ThrmlZn:Desc If  ThrmlZn:PriAirCondgSysRef OR ExhSysRef  OR VentSysRef  == ZnSys:Desc in Zone01</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Zone03_TerminalUnitType" type="hvac:TerminalUnitType" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">System Type</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>TrmlUnit:Type</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Zone03_ZoneSystemType" type="hvac:ZoneSystemType" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">System Type</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ZnSys:Type</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Zone04_TerminalUnitCount" type="hvac:TerminalUnitCount" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Qty</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>TrmlUnit:SysCnt</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Zone04_ZoneSysCount" type="hvac:ZoneSysCount" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Qty</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ZnSys:Cnt</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="Zone05to06ColLabel" minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Rated Capacity (kBtuh)</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Zone05_HeatingRatedCapacity" type="hvac:HeatingRatedCapacity" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Heating</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ZnSys:CoilHtg:CapTotGrossRtd Or CapTotNetRtd Or ThrmlUnit:CoilHtg:CapTotNetRtd ??</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Zone05_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Heating</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Zone06_CoolingRatedCapacity" type="hvac:CoolingRatedCapacity" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Cooling</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ZnSys:CoilClg:CapTotGrossRtd Or CapTotNetRtd Or ThrmlUnit:CoilClg:CapTotNetRtd ??</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Zone06_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Cooling</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="Zone07to09ColLabel" minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Airflow (cfm)</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Zone07_RatedFlowCapacity" type="hvac:RatedFlowCapacity" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Design</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>If AirSys:ThrmlUnit  result = AirSys:ThrmlUnit:PriAirFlowMax Else If ZnSys  See conditions Either result = ZnSys:RtdFlowCap Or result = ZnSys:Fan:FlowCap End If</dtyp:CBECCnrXMLterm>
                                                <dtyp:termConditions>if ZnSys:Type = "VentilatonOnly"
result = ZnSys:RtdFlowCap
else
result = ZnSys:Fan:FlowCap
end if
</dtyp:termConditions>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Zone07_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Design</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Zone08_AirFlowMinimum" type="hvac:AirFlowMinimum" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">MIn.</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ZnSys:Fan:FlowMin or AirSys:TrmlUnit:PriAirFlowMin</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Zone08_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">MIn.</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Zone09_DesignAirFlowMinimumRatio" type="hvac:DesignAirFlowMinimumRatio" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Min. Ratio</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ZnSys:Fan:FlowMinFrac or AirSys:TrmlUnit:PriAirFlowMinFrac</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Zone09_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Min. Ratio</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="Zone10to13ColLabel" minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Fan</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Zone10_MotorBHP" type="hvac:MotorBHP" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Power</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>If ZnSys result = ZnSys:Fan:MtrBHP  Else If TrmlUnit result = TrmlUnit:Fan:MtrBHP</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Zone10_FanPowerModeled" type="hvac:FanPowerModeled" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Power</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>If ZnSys: See conditions result = ZnSys:Fan:PwrRpt  Or ZnSys:RtdPwrIdx    Else If TrmlUnit  result =  TrmlUnit:Fan:PwrRpt</dtyp:CBECCnrXMLterm>
                                                <dtyp:termConditions>if ZnSys:Type = "VentilatonOnly"
result = ZnSys:RtdPwrIdx
else
result = ZnSys:Fan:PwrRpt
end If</dtyp:termConditions>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Zone10_TotalStaticPressure" type="hvac:TotalStaticPressure" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Power</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>AirSys:AirSeg:Fan:PwrRpt Or ZnSys:Fan:PwrRpt</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Zone10_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Power</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Zone11_FanPowerUnits" type="hvac:FanPowerUnits" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Power Units</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>If AirSys result = AirSys:AirSeg:Fan:PwrUnitRpt Else If ZnSys See conditions result = Watts Or ZnSys:Fan:PwrUnitRpt End If</dtyp:CBECCnrXMLterm>
                                                <dtyp:termConditions>if ZnSys:Type = "VentilatonOnly"
result = Watts
else
result = ZnSys:Fan:PwrUnitRpt</dtyp:termConditions>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Zone11_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Power Units</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Zone12_FanControl" type="hvac:FanControl" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Cycles</xsd:documentation>
                                             <xsd:appinfo source="displayterm">
                                                <dtyp:displayterm value="Continuous">No</dtyp:displayterm>
                                                <dtyp:displayterm value="Cycling">Yes</dtyp:displayterm>
                                             </xsd:appinfo>
                                             
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ZnSys:FanCtrl If ZnSys:Desc in Zone01 Or TrmlUnit parent AirSys:FanCtrl If TrmlUnit in Zone01</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Zone12_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Cycles</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="Zone13_IsVSD" type="hvac:IsVSD" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">VSD</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Fan:CtrlMthd  If Fan:CtrlMthd == VariableSpeedDrive result = true Else result = false</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_Evap" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]H12. EVAPORATIVE COOLER SUMMARY[/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableEvap" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Evap01_EvaporativeCoolerName" type="hvac:EvaporativeCoolerName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">System ID</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>AirSys:AirSeg:EvapClr:Name</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Evap02_EvaporativeCoolerType" type="hvac:EvaporativeCoolerType" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Type</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>AirSys:AirSeg:EvapClr:Type</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Evap03_EvaporativeCoolerCount" type="hvac:EvaporativeCoolerCount" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Qty</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>AirSys:AirSeg:EvapClr:SysCnt</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Evap04_Effectiveness" type="hvac:Effectiveness" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Effectiveness</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>:AirSys:AirSeg:EvapClr:Eff</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Evap05_PumpPower" type="hvac:PumpPower" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Pump Power (Watts)</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>AirSys:AirSeg:EvapClr:PumpPwr</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Evap05_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Pump Power (Watts)</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>AirSys:AirSeg:EvapClr:PumpPwr</dtyp:CBECCnrXMLterm>
