                                             <dtyp:termConditions>If Proj:FenCons:Status == New 
result = Proj:FenCons:AreaNew
Else If Proj:FenCons:Status == Existing 
result = Proj:FenCons:AreaExisting
Else If Proj:FenCons:Status == Altered 
result = Proj:FenCons:AreaAltered
End If
</dtyp:termConditions>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Fen09mf_FenestrationAssemblyUFactor" type="env:FenestrationAssemblyUFactor" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Overall U-factor</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Use this choice If Fen03_FenestrationCertificationMethod != COGEquations
End If</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>Proj:FenCons:Ufactor</dtyp:CBECCnrXMLterm>
                                                <dtyp:termConditions>If Proj:FenCons:CertificationMthd != COGEquations
result = Proj:FenCons:Ufactor</dtyp:termConditions>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Fen09mf_UFactorCenterOfGlass" type="com:UFactorCenterOfGlass" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Overall U-factor</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Use this choice If Fen03_FenestrationCertificationMethod == COGEquations
End If</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>Proj:FenCons:UfactorCOG</dtyp:CBECCnrXMLterm>
                                                <dtyp:termConditions>If Proj:FenCons:CertificationMthd == COGEquations
result = Proj:FenCons:UfactorCOG</dtyp:termConditions>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Fen10mf_FenestrationCertificationMethod" type="env:FenestrationCertificationMethod" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">U-factor Source</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Fen10mf_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">U-factor Source</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Fen11mf_FenestrationAssemblySHGC" type="env:FenestrationAssemblySHGC" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Overall SHGC</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>Proj:FenCons:SHGC</dtyp:CBECCnrXMLterm>
                                                <dtyp:termConditions>If Proj:FenCons:CertificationMthd != COGEquations
result = Proj:FenCons:SHGC</dtyp:termConditions>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Fen11mf_SHGC_CenterOfGlass" type="com:SHGC_CenterOfGlass" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Overall SHGC</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>Proj:FenCons:SHGCCOG</dtyp:CBECCnrXMLterm>
                                                <dtyp:termConditions>If Proj:FenCons:CertificationMthd == COGEquations
result = Proj:FenCons:SHGCCOG</dtyp:termConditions>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Fen12mf_FenestrationCertificationMethod" type="env:FenestrationCertificationMethod" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">SHGC Source</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Fen12mf_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">SHGC Source</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Fen13mf_FenestrationAssemblyVT" type="env:FenestrationAssemblyVT" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Overall VT</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>Proj:FenCons:VT</dtyp:CBECCnrXMLterm>
                                                <dtyp:termConditions>If Proj:FenCons:CertificationMthd != COGEquations
result = Proj:FenCons:VT</dtyp:termConditions>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Fen13mf_VT_CenterOfGlass" type="com:VT_CenterOfGlass" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Overall VT</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>Proj:FenCons:VTCOG</dtyp:CBECCnrXMLterm>
                                                <dtyp:termConditions>If Proj:FenCons:CertificationMthd == COGEquations
result = Proj:FenCons:VTCOG</dtyp:termConditions>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Fen13mf_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Overall VT</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Fen14mf_ExternalShadingObjectType" type="env:ExternalShadingObjectType" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Exterior Shading</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Fen14mf_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Exterior Shading</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="Fen15mf_Status" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Status[d:sup]1[/d:sup]</xsd:documentation>
                                          <xsd:appinfo source="displayterm">
                                             <dtyp:displayterm value="ExistingAlteration">A</dtyp:displayterm>
                                             <dtyp:displayterm value="ExistingUnchanged">E</dtyp:displayterm>
                                             <dtyp:displayterm value="New">N</dtyp:displayterm>
                                          </xsd:appinfo>
                                          
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Proj:FenCons:Status</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="com:Status">
                                             <xsd:enumeration value="ExistingAlteration"/>
                                             <xsd:enumeration value="ExistingUnchanged"/>
                                             <xsd:enumeration value="New"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="FenmfFootNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:i][d:sup]1[/d:sup] Status: N - New, A - Altered, E - Existing[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_OverHang" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]G8. OVERHANG DETAILS [/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableOverHang" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="OverHang01_FenestrationName" type="env:FenestrationName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Fenestration Tag/ ID</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Proj:Bldg:Story;Spc:ExtWall:Win:Name  Or :ResExtWall:ResWin:Name</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="OverHang02_Azimuth" type="dtyp:Azimuth" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Azimuth</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Proj:Bldg:Story;Spc:ExtWall:Win:OrientationRpt Or ResExtWall:ResWin:Azimuth</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="OverHang03_OverhangDepth" type="com:OverhangDepth" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Depth (ft)</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Proj:Bldg:Story;Spc:ExtWall:Win:OverhangDepth Or :ResExtWall:ResWin:OverhangDepth</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="OverHang04_OverhangHeight" type="env:OverhangHeight" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Height from Top of Sill to Overhang (ft)</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Proj:Bldg:Story;Spc:ExtWall:Win:Y + Proj:Bldg:Story;Spc:ExtWall:Win:Hgt + Proj:Bldg:Story;Spc:ExtWall:Win:OverhangDistance  Or ResExtWall:ResWin:Height + ResExtWall:ResWin:OverhangDistUp</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="OverHang05_OverhangRightExtent" type="env:OverhangRightExtent" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Right Extent (ft)</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Proj:Bldg:Story;Spc:ExtWall:Win:OverhangRightExtension or :ResExtWall:ResWin:OverhangExR</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="OverHang06_OverhangLeftExtent" type="env:OverhangLeftExtent" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Left Extent (ft)</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Proj:Bldg:Story;Spc:ExtWall:Win:OverhangLeftExtension or ResExtWall:ResWin:OverhangExL</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="OverHang07_OverhangFlapHeight" type="env:OverhangFlapHeight" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Flap Height</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ResExtWall:ResWin:OverhangFlap</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="OverHang07_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Flap Height</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_Fins" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]G9. FIN DETAILS [/d:b]</xsd:documentation>
               <xsd:appinfo source="CBECCnrXML">
                  <dtyp:CBECCnrXMLterm>This section is required if any condition is true</dtyp:CBECCnrXMLterm>
                  <dtyp:termConditions>Proj:Bldg:Story;Spc:ExtWall:Win:RightFinDepth GT 0
Proj:Bldg:Story;Spc:ExtWall:Win:LeftFinDepth GT 0</dtyp:termConditions>
