         <xsd:element name="Section_WaterMF" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]I2. MULTI-FAMILY WATER HEATING SYSTEM DETAIL [/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableWaterMF" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="WaterMF01_WaterHeatingSystemName" type="hvac:WaterHeatingSystemName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">System Name</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>ResDHWSys:Name</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="WaterMF02_ResidentialWaterHeatingSystemType" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Configuration</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Result = DHW</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Domestic Hot Water (DHW)</dtyp:CBECCnrXMLterm>
                                             <dtyp:termConditions>Domestic Hot Water (DHW)</dtyp:termConditions>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="hvac:ResidentialWaterHeatingSystemType">
                                             <xsd:enumeration value="DHW"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:element name="WaterMF03_CentralSystem" type="hvac:CentralSystem" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Type</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>ResDHWSys:CentralSys  If ResDHWSys:CentralSys == 1 result = Central Else If ResDHWSys:CentralSys == 0 result =  Unitary</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="WaterMF04_WaterHeaterTotalSystemCount" type="hvac:WaterHeaterTotalSystemCount" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Qty in System</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>ResDHWSys:WtrHtrCnt</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="WaterMF05_ResidentialDHWCentralDistributionType" type="hvac:ResidentialDHWCentralDistributionType" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                             <xsd:documentation source="AdditionalRequirements">Multi-Family Distribution Type</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Water03_CentralSystem == true use this choice</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ResDHWSys:DistType;  If ResDHWSys:CentralSys == 1 use this choice Else If ResDHWSys:CentralSys == 0 result = NA</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="WaterMF05_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Water03_CentralSystem == false result = NotApplicable</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="WaterMF06_ResidentialDHWDwellingUnitDistributionType" type="hvac:ResidentialDHWDwellingUnitDistributionType" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Dwelling Unit Distribution Type</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Water03_CentralSystem == false use this result</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ResDHWSys:Type;  If ResDHWSys:CentralSys == 0 use this choice Else If ResDHWSys:CentralSys == 1 result = NA</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="WaterMF06_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Dwelling Unit Distribution Type</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Water03_CentralSystem == true result = NotApplicable</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="WaterMF07_WaterHeaterName" type="hvac:WaterHeaterName"  maxOccurs="10">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Water Heater Name</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ResDHWSys:ResWtrHtr:Name</dtyp:CBECCnrXMLterm>
                                                <dtyp:termConditions>if ResDHWSys:CentralSys=1 AND ResDHWSys:CentralSysType="HPWH" then ResDHWSys:Name+" - heater"
Else result is one or more ResDHWSys:ResWtrHtr:Name</dtyp:termConditions>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="WaterMF07_WaterHeatingSystemName" type="hvac:WaterHeatingSystemName"  maxOccurs="1">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Water Heater Name (#)</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ResDHWSys:Name+" - heater"</dtyp:CBECCnrXMLterm>
                                                <dtyp:termConditions>if ResDHWSys:CentralSys=1 AND ResDHWSys:CentralSysType="HPWH" then ResDHWSys:Name+" - heater"
</dtyp:termConditions>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="WaterMF07a_WaterHeaterCount" type="hvac:WaterHeaterCount"  minOccurs="0" maxOccurs="10">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Water Heater Name (#)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If WaterMF07_WaterHeaterName IsPresent, this field is required to record the number of that water heater in () after the name</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="WaterMF08_DHWSolarSystemName" type="hvac:DHWSolarSystemName" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Solar Heating System</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Proj05_PerfSolarWH == true use this choice</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ResDHWSys:DHWSolarSysRef;  If ResDHWSys:CSESolarSysSimValid == 1  use this choice Else If ResDHWSys:CSESolarSysSimValid != 1 result = NA</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="WaterMF08_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Solar Heating System</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Proj05_PerfSolarWH == false result = NotAppliacable</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="WaterMF09_IsCompactDistribution" type="hvac:IsCompactDistribution" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Is Compact Distribution</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="WaterMF09_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Is Compact Distribution</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="WaterMF10_PipeInsulationVerificationRequired" type="comp:PipeInsulationVerificationRequired" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                             <xsd:documentation source="AdditionalRequirements">Pipe Insulation Verification Required</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Fields WaterMF10 - 12 come from CF1RPRF schema Section_U WATER HEATING - HERS VERIFICATION table</xsd:documentation>
                                         </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="WaterMF10_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                             <xsd:documentation source="AdditionalRequirements">Pipe Insulation Verification Required</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="WaterMF11_ParallelPipingVerificationRequired" type="comp:ParallelPipingVerificationRequired" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                             <xsd:documentation source="AdditionalRequirements">Parallel Piping Verification Required</xsd:documentation>
                                         </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="WaterMF11_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                             <xsd:documentation source="AdditionalRequirements">Parallel Piping Verification Required</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="WaterMF12_RecirculationControlVerificationRequired" type="comp:RecirculationControlVerificationRequired" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                             <xsd:documentation source="AdditionalRequirements">Recirculation Control Verification Required</xsd:documentation>
                                         </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="WaterMF12_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                             <xsd:documentation source="AdditionalRequirements">Recirculation Control Verification Required</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="WaterMF13_DWHR_HERS_VerificationRequired" type="comp:DWHR_HERS_VerificationRequired" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                             <xsd:documentation source="AdditionalRequirements">DWHR Credit Taken</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="WaterMF13_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                             <xsd:documentation source="AdditionalRequirements">DWHR Credit Taken</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_WaterCHPWH" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]I3.  MULTIFAMILY &amp; HOTEL/MOTEL WATER HEATER EQUIPMENT SUMMARY - CHPWH [/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableCHPWH" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="WaterCHPWH01_WaterHeatingSystemName" type="hvac:WaterHeatingSystemName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Name</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>ResDHWSys:Name</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="WaterCHPWH02" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"></xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:complexType>
                                          <xsd:choice >
                                             <xsd:element name="WaterCHPWH02_CentralHPWHCompressorType" type="hvac:CentralHPWHCompressorType" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Brand/Model</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">If Water13_WaterHeaterTypeNR != HeatPumpPackaged this single field is the choice to provide</xsd:documentation>
                                                   <xsd:appinfo source="CBECCnrXML">
                                                      <dtyp:CBECCnrXMLterm>ResDHWSys:CentralHPWHCprsr</dtyp:CBECCnrXMLterm>
                                                      <dtyp:termConditions>If ResDHWSys:CentralHTypePWHSysDesc != "Integrated, Packaged System" Then result = ResDHWSys:CentralHPWHCprsr</dtyp:termConditions>
                                                   </xsd:appinfo>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="WaterCHPWH02_HeatPumpWaterHeaterModel" type="hvac:HeatPumpWaterHeaterModel" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Brand/Model</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">If Water13_WaterHeaterTypeNR == HeatPumpPackaged this field is provided for a commercial product</xsd:documentation>
                                                   <xsd:appinfo source="CBECCnrXML">
                                                      <dtyp:CBECCnrXMLterm>ResDHWSys:CentralHPWHCommModel</dtyp:CBECCnrXMLterm>
                                                      <dtyp:termConditions>If ResDHWSys:CentralHTypePWHSysDesc == "Integrated, Packaged System" And ResDHWSys:CentralHPWHIntegratedPackageType == "Commercial Product" Then result = ResDHWSys:CentralHPWHCommModel</dtyp:termConditions>
                                                   </xsd:appinfo>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="WaterCHPWH02_HeatPumpWaterHeaterBrandModel" type="hvac:HeatPumpWaterHeaterBrandModel" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Brand/Model</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">String value</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:sequence maxOccurs="1">
                                                <xsd:annotation>
                                                   <xsd:appinfo source="CBECCnrXML">
                                                      <dtyp:CBECCnrXMLterm>ResDHWSys:CentralHPWHBrand;</dtyp:CBECCnrXMLterm>
                                                      <dtyp:termConditions>If ResDHWSys:CentralHTypePWHSysDesc == "Integrated, Packaged System" And ResDHWSys:CentralHPWHIntegratedPackageType == "Residential (NEEA rated) Product" Then result = ResDHWSys:CentralHPWHBrand / ResDHWSys:CentralHPWHModel</dtyp:termConditions>
                                                   </xsd:appinfo>
                                                </xsd:annotation>
                                                <xsd:element name="WaterCHPWH02_HeatPumpWaterHeaterBrand" type="hvac:HeatPumpWaterHeaterBrand" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Brand/Model</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="WaterCHPWH02_HeatPumpWaterHeaterModel" type="hvac:HeatPumpWaterHeaterModel"  maxOccurs="1">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Brand/Model</xsd:documentation>
                                                      <xsd:appinfo source="CBECCnrXML">
                                                         <dtyp:CBECCnrXMLterm>ResDHWSys:CentralHPWHModel;</dtyp:CBECCnrXMLterm>
                                                         <dtyp:termConditions>If ResDHWSys:CentralHTypePWHSysDesc == "Integrated, Packaged System"  And ResDHWSys:CentralHPWHIntegratedPackageType == "Residential (NEEA rated) Product" Then result =  ResDHWSys:CentralHPWHBrand / ResDHWSys:CentralHPWHModel</dtyp:termConditions>
                                                      </xsd:appinfo>
                                                   </xsd:annotation>
                                                </xsd:element>
                                             </xsd:sequence>
                                          </xsd:choice>
                                       </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="WaterCHPWH02a_HeatPumpWaterHeaterSimulationGroup" type="hvac:HeatPumpWaterHeaterSimulationGroup"  minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="WaterCHPWH02b_WaterHeaterFuelSource" type="hvac:WaterHeaterFuelSource"  minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                          <xsd:documentation source="AdditionalRequirements">Heater Element Type</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>ResDHWSys:ResWtrHtr:ElementType Or  FluidSys:WtrHtr:FuelSrc</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="WaterCHPWH03_CompressorCountCHPWH" type="hvac:CompressorCountCHPWH" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Number of Compressors</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>ResDHWSys:CentralHPWHCprsrCnt</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="WaterCHPWH04_PrimaryTankCapacity" type="hvac:PrimaryTankCapacity" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Primary Tank Volume (gal)</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>ResDHWSys:CentralHPWHPriTankVol</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="WaterCHPWH05_TankCountCHPWH" type="hvac:TankCountCHPWH" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Tank Count</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ResDHWSys:CentralHPWHPriTankCnt</dtyp:CBECCnrXMLterm>
                                                <dtyp:termConditions>If ResDHWSys:CentralHTypePWHSysDesc != "Integrated, Packaged System" result = ResDHWSys:CentralHPWHPriTankCnt</dtyp:termConditions>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="WaterCHPWH05_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Tank Count</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Water13_WaterHeaterTypeNR == HeatPumpPackaged result = NotApplicable</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>NotApplicable</dtyp:CBECCnrXMLterm>
                                                <dtyp:termConditions>If ResDHWSys:CentralHTypePWHSysDesc == "Integrated, Packaged System" result = NA</dtyp:termConditions>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="WaterCHPWH06_PrimaryTankTotalInsulation" type="hvac:PrimaryTankTotalInsulation" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Tank R-value</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ResDHWSys:CentralHPWHTankRVal</dtyp:CBECCnrXMLterm>
                                                <dtyp:termConditions>If ResDHWSys:CentralHTypePWHSysDesc != "Integrated, Packaged System" result = ResDHWSys:CentralHPWHTankRVal</dtyp:termConditions>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="WaterCHPWH06_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Tank R-value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Water13_WaterHeaterTypeNR == HeatPumpPackaged result = NotApplicable</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>NotApplicable</dtyp:CBECCnrXMLterm>
                                                <dtyp:termConditions>If ResDHWSys:CentralHTypePWHSysDesc == "Integrated, Packaged System" result = NA</dtyp:termConditions>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="WaterCHPWH07_TankLocation" type="hvac:TankLocation" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Tank Location</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ResDHWSys:CentralHPWHTankLctn</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="WaterCHPWH07_ZoneSystemName" type="hvac:ZoneSystemName" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Tank Location</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>Zone:Name</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="WaterCHPWH08_AirSource" type="hvac:AirSource" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Air Source</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ResDHWSys:CentralHPWHAirSrc</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="WaterCHPWH08_ZoneSystemName" type="hvac:ZoneSystemName" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Air Source</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>Zone:Name</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_LpTank" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]I4. MULTIFAMILY &amp; HOTEL/MOTEL WATER HEATER EQUIPMENT SUMMARY - CHPWH SECONDARY TANK [/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableLpTank" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="LpTank01_LoopTankType" type="hvac:LoopTankType" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Tank Type</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>ResDHWSys:CentralHPWHLpTankType</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="LpTank01a_LoopTankConfig" type="hvac:LoopTankConfig" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Tank Configuration</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="LpTank01b_WaterHeatingSystemName" type="hvac:WaterHeatingSystemName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>ResDHWSys:Name</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="LpTank02" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"></xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:complexType>
                                          <xsd:choice >
                                             <xsd:element name="LpTank02_CentralHPWHCompressorType" type="hvac:CentralHPWHCompressorType" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Brand/Model</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">If LpTank01_LoopTankType == LargeCompressor use this choice</xsd:documentation>
                                                   <xsd:appinfo source="CBECCnrXML">
                                                      <dtyp:CBECCnrXMLterm>ResDHWSys:CentralHPWHLpTankCprsrType</dtyp:CBECCnrXMLterm>
                                                      <dtyp:termConditions>If ResDHWSys:CentralHPWHLpTankType == Large HPWH/Compressor result = ResDHWSys:CentralHPWHLpTankCprsrType</dtyp:termConditions>
                                                   </xsd:appinfo>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="LpTank02_HeatPumpWaterHeaterModel" type="hvac:HeatPumpWaterHeaterModel" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Brand/Model</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">If LpTank01_LoopTankType == CommercialHPWH use this choice</xsd:documentation>
                                                   <xsd:appinfo source="CBECCnrXML">
                                                      <dtyp:CBECCnrXMLterm>ResDHWSys:CentralHPWHLpTankCommModel</dtyp:CBECCnrXMLterm>
                                                      <dtyp:termConditions>If ResDHWSys:CentralHPWHLpTankType == "Commercial HPWH"  result = ResDHWSys:CentralHPWHLpTankCommModel</dtyp:termConditions>
                                                   </xsd:appinfo>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="LpTank02_HeatPumpWaterHeaterBrandModel" type="hvac:HeatPumpWaterHeaterBrandModel" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Brand/Model</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">String value</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:sequence minOccurs="1" maxOccurs="1">
                                                <xsd:annotation>
                                                   <xsd:documentation source="CalculationsAndRules">If LpTank01_LoopTankType == NEEA use this choice</xsd:documentation>
                                                   <xsd:appinfo source="CBECCnrXML">
                                                      <dtyp:CBECCnrXMLterm>ResDHWSys:CentralHPWHLpTankBrand</dtyp:CBECCnrXMLterm>
                                                      <dtyp:termConditions>If ResDHWSys:CentralHPWHLpTankType == NEEA HPWH  result = ResDHWSys:CentralHPWHLpTankBrand / ResDHWSys:CentralHPWHLpTankModel</dtyp:termConditions>
                                                   </xsd:appinfo>
                                                </xsd:annotation>
                                                <xsd:element name="LpTank02_HeatPumpWaterHeaterBrand" type="hvac:HeatPumpWaterHeaterBrand" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Brand/Model</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="LpTank02_HeatPumpWaterHeaterModel" type="hvac:HeatPumpWaterHeaterModel"  minOccurs="1" maxOccurs="1">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Brand/Model</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If LpTank01_LoopTankType == NEEA use this choice</xsd:documentation>
                                                      <xsd:appinfo source="CBECCnrXML">
                                                         <dtyp:CBECCnrXMLterm>ResDHWSys:CentralHPWHLpTankModel</dtyp:CBECCnrXMLterm>
                                                         <dtyp:termConditions>If ResDHWSys:CentralHPWHLpTankType == NEEA HPWH result = ResDHWSys:CentralHPWHLpTankBrand / ResDHWSys:CentralHPWHLpTankModel</dtyp:termConditions>
                                                      </xsd:appinfo>
                                                   </xsd:annotation>
                                                </xsd:element>
                                             </xsd:sequence>
                                             <xsd:element name="LpTank02_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Brand/Model</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">If LpTank01_LoopTankType != LargeCompressor And LpTank01_LoopTankType != NEEA result = NotApplicable</xsd:documentation>
                                                   <xsd:appinfo source="CBECCnrXML">
                                                      <dtyp:CBECCnrXMLterm>NotApplicable</dtyp:CBECCnrXMLterm>
                                                      <dtyp:termConditions>If ResDHWSys:CentralHPWHLpTankType != Large HPWH/Compressor And ResDHWSys:CentralHPWHLpTankType != NEEA HPWH result = NA</dtyp:termConditions>
                                                   </xsd:appinfo>
                                                </xsd:annotation>
                                             </xsd:element>
                                          </xsd:choice>
                                       </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="LpTank03_CompressorCountCHPWH" type="hvac:CompressorCountCHPWH" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Count</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>ResDHWSys:CentralHPWHLpTankCprsrCnt</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="LpTank04_LoopTankCapacity" type="hvac:LoopTankCapacity" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Tank Volume (gal)</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>ResDHWSys:CentralHPWHLpTankVol</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="LpTank05_TankCountCHPWH" type="hvac:TankCountCHPWH" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Tank Count</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If LpTank01_LoopTankType !=  NEEA use this choice</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ResDHWSys:CentralHPWHLpTankCnt</dtyp:CBECCnrXMLterm>
                                                <dtyp:termConditions>If ResDHWSys:CentralHPWHLpTankType != NEEA HPWH And  ResDHWSys:CentralHPWHLpTankType != Commercial HPWH result = ResDHWSys:CentralHPWHLpTankCnt</dtyp:termConditions>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="LpTank05_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Tank Count</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If LpTank01_LoopTankType == NEEA result = NotApplicable</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>NotApplicable</dtyp:CBECCnrXMLterm>
                                                <dtyp:termConditions>If ResDHWSys:CentralHPWHLpTankType == NEEA HPWH Or Commercial HPWH result = NA</dtyp:termConditions>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="LpTank06_LoopTankTotalInsulation" type="hvac:LoopTankTotalInsulation" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Tank R-value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If LpTank01_LoopTankType !=  NEEA use this choice</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ResDHWSys:CentralHPWHLpTankRVal</dtyp:CBECCnrXMLterm>
                                                <dtyp:termConditions>If ResDHWSys:CentralHPWHLpTankType != NEEA HPWH And  ResDHWSys:CentralHPWHLpTankType != Commercial HPWH result = ResDHWSys:CentralHPWHLpTankRVal</dtyp:termConditions>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="LpTank06_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Tank R-value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If LpTank01_LoopTankType == NEEA result = NotApplicable</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>NotApplicable If ResDHWSys:CentralHPWHLpTankType == NEEA HPWH</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="LpTank07_LoopTankLocation" type="hvac:LoopTankLocation" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Tank Location</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>ResDHWSys:CentralHPWHLpTankLctn</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="LpTank08_AirSource" type="hvac:AirSource" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Air Source</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If LpTank01_LoopTankType !=  ElectricResistance use this choice</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ResDHWSys:CentralHPWHLpTankAirSrc  If ResDHWSys:CentralHPWHLpTankType != Electric Resistance</dtyp:CBECCnrXMLterm>
                                                <dtyp:termConditions>If ResDHWSys:CentralHPWHLpTankType != Electric Resistance Then 
result =ResDHWSys:CentralHPWHLpTankAirSrc
End If</dtyp:termConditions>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="LpTank08_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Air Source</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If LpTank01_LoopTankType == ElectricResistance result = NotApplicable</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>NotApplicable</dtyp:CBECCnrXMLterm>
                                                <dtyp:termConditions>If ResDHWSys:CentralHPWHLpTankType == Electric Resistance result = NA</dtyp:termConditions>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_RecircLp" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]I5. RECIRCULATION LOOPS [/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableRecircLp" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="RecircLp01_WaterHeatingSystemName" type="hvac:WaterHeatingSystemName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Water Heating System Name</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>ResDHWSys:Name</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="RecircLp02_RecirculationLoopCount" type="hvac:RecirculationLoopCount" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Number of Recirculation Loops</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>ResDHWSys:LpCnt</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="RecircLp03_PipeInsulationThickness" type="hvac:PipeInsulationThickness" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Loop Insulation Thickness (in)</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>ResDHWSys:LpPipeInsThkns[0]</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="RecircLp04_PipeLocation" type="hvac:PipeLocation" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Recirculation Loop Location</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>ResDHWSys:PipeLctn</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="RecircLp05_RecirculationPumpPower" type="hvac:RecirculationPumpPower" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Recirculation Pump Power</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>ResDHWSys:RecircPumpPwr</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="RecircLp06_PumpPowerUnits" type="hvac:PumpPowerUnits" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Recirculation Pump Power Units</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>ResDHWSys:RecircPumpPwrUnits</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
