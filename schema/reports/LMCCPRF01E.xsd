                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Total Unconditioned Floor Area (ft[d:sup]2[/d:sup])</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Info17_SiteFuelType" type="com:SiteFuelType" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Fuel Type</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Info18_NonResBuildingTotalConditionedFloorArea" type="com:NonResBuildingTotalConditionedFloorArea" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Nonresidential Conditioned Floor Area</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Info19_AboveGradeStoryCount" type="env:AboveGradeStoryCount" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Total # of Stories (Habitable Above Grade)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Info20_ResidentialBuildingTotalConditionedFloorArea" type="com:ResidentialBuildingTotalConditionedFloorArea" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Residential Conditioned Floor Area</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="Info21_BuildingTotalVolume" type="com:BuildingTotalVolume" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Total Building Volume</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="Info21_NotApplicableMessage" type="comp:NotApplicableMessage" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Total Building Volume</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:choice>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_Proj" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]B. PROJECT SUMMARY [/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="Proj_BeginNote" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:i]Table B shows which building components are included in the performance calculation.  If indicated as not included, the project must show compliance prescriptively if within the permit application.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="ProjSubheading" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Building Components Complying via Performance</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Proj01_PerfENV" type="comp:PerfENV" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Envelope (See Table G)</xsd:documentation>
                        <xsd:appinfo source="CBECCnrXML">
                           <dtyp:CBECCnrXMLterm>Proj:CompPathEnv</dtyp:CBECCnrXMLterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Proj01mf_PerfENV" type="comp:PerfENV" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Envelope (See Table G)</xsd:documentation>
                        <xsd:appinfo source="CBECCnrXML">
                           <dtyp:CBECCnrXMLterm>Proj:ResCompPathEnv</dtyp:CBECCnrXMLterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Proj02_PerfMCH" type="comp:PerfMCH" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Mechanical (See Table H)</xsd:documentation>
                        <xsd:appinfo source="CBECCnrXML">
                           <dtyp:CBECCnrXMLterm>Proj:CompPathMech</dtyp:CBECCnrXMLterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Proj02mf_PerfMCH" type="comp:PerfMCH" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Mechanical (See Table H)</xsd:documentation>
                        <xsd:appinfo source="CBECCnrXML">
                           <dtyp:CBECCnrXMLterm>Proj:ResCompPathMech</dtyp:CBECCnrXMLterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Proj03_PerfDHW" type="comp:PerfDHW" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Domestic Hot Water (See Table I)</xsd:documentation>
                        <xsd:appinfo source="CBECCnrXML">
                           <dtyp:CBECCnrXMLterm>Proj:CompPathDHW</dtyp:CBECCnrXMLterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Proj03mf_PerfDHW" type="comp:PerfDHW" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Domestic Hot Water (See Table I)</xsd:documentation>
                        <xsd:appinfo source="CBECCnrXML">
                           <dtyp:CBECCnrXMLterm>Proj:ResCompPathDHW</dtyp:CBECCnrXMLterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Proj04_PerfCondLTI" type="comp:PerfCondLTI" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Lighting (Indoor Conditioned, see Table K)</xsd:documentation>
                        <xsd:appinfo source="CBECCnrXML">
                           <dtyp:CBECCnrXMLterm>Proj:CompPathLtg</dtyp:CBECCnrXMLterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Proj04mf_PerfCondLTI" type="comp:PerfCondLTI" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Lighting (Indoor Conditioned, see Table K)</xsd:documentation>
                        <xsd:appinfo source="CBECCnrXML">
                           <dtyp:CBECCnrXMLterm>Proj:ResCompPathLtg</dtyp:CBECCnrXMLterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Proj05_PerfSolarWH" type="comp:PerfSolarWH" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Solar Thermal Water Heating (See Table I3)</xsd:documentation>
                        <xsd:appinfo source="CBECCnrXML">
                           <dtyp:CBECCnrXMLterm>Proj:SolThrmlComp</dtyp:CBECCnrXMLterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Proj06_PerfKitchenPRC" type="comp:PerfKitchenPRC" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Covered Process: Commercial Kitchens (see Table J)</xsd:documentation>
                        <xsd:appinfo source="CBECCnrXML">
                           <dtyp:CBECCnrXMLterm>Proj:CompPathKit</dtyp:CBECCnrXMLterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Proj08_PerfLabPRC" type="comp:PerfLabPRC" >
