                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="CRRC01mf_PartitionName" type="env:PartitionName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Name</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="CRRC02mf_RoofSlope" type="env:RoofSlope" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Roof Pitch</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="CRRC02amf_RoofRise" type="env:RoofRise" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Roof Rise (x in 12)</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="CRRC03mf_SolarReflectanceAged" type="com:SolarReflectanceAged" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Aged Solar Reflectance</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>ConsAssm:CRRCAgedRefl</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="CRRC04mf_ThermalEmittance" type="com:ThermalEmittance" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Thermal Emittance</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>ConsAssm:CRRCAgedEmit</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="CRRC05mf_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">SRI</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_Attic" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">G3. ATTIC</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableAttic" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Attic01_AtticName" type="env:AtticName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Name</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Proj:Bldg:ResZnGrp:ResAttic:Name</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Attic02_ConstructionAssemblyNamePerformanceRes" type="comp:ConstructionAssemblyNamePerformanceRes" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Construction</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Proj:Bldg:ResZnGrp:ResAttic:Construction</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Attic03_IsAtticVented" type="comp:IsAtticVented" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Type</xsd:documentation>
                                          <xsd:appinfo source="displayterm">
                                             <dtyp:displayterm value="false">Unventilated</dtyp:displayterm>
                                             <dtyp:displayterm value="true">Ventilated</dtyp:displayterm>
                                          </xsd:appinfo>
                                          
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Proj:Bldg:ResZnGrp:ResAttic:Type</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Attic07_RadiantBarrierToBeInstalled" type="comp:RadiantBarrierToBeInstalled" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Radiant Barrier</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Cons:RadiantBarrier</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_AirBar" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">G4. NONRESIDENTIAL AIR BARRIER</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">If Info10_TopLevelOccupancy only contains LowRiseResidential this section is not requird</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableAirBar" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="AirBar01_BuildingStoryName" type="com:BuildingStoryName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Building Story Name</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Story:Name</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="AirBar02_StoryAirBarrier" type="comp:StoryAirBarrier" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Air Barrier</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Story:AirBarrier</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
