   <xsd:element name="ComplianceDocumentPackage" id="Sign1">
      <xsd:complexType>
         <xsd:sequence>
            <xsd:element name="DocID" minOccurs="0">
               <xsd:complexType>
                  <xsd:attribute name="doc" type="comp:ComplianceDocumentTag" fixed="LMCCPRF01E"/>
                  <xsd:attribute name="docType" type="comp:ComplianceDocumentType" fixed="CERTIFICATE OF COMPLIANCE -  LOWRISE MULTIFAMILY MIXED USE PERFORMANCE COMPLIANCE METHOD"/>
                  <xsd:attribute name="docTitle" type="comp:ComplianceDocumentTitleRes" fixed="LMCC Performance Compliance"/>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Payload" type="comp:Payload" minOccurs="0"/>
            <xsd:element name="DocumentData">
               <xsd:complexType>
                  <xsd:sequence>
                     <xsd:element name="Header" type="comp:HeaderCF1RPerf" />
                     <xsd:element ref="lMCCPRF01E" />
                     <xsd:element name="ProjectNotes" type="comp:ProjectNotes"  minOccurs="0"/>
                     <xsd:element name="DeclarationsBlock" type="comp:DeclarationsNRCCPRF" maxOccurs="unbounded"/>
                     <!-- Disabled for consistency with NRCC - RScott - 11-11-2022 -->
                     <!--<xsd:element name="DocAuthor" type="comp:DocumentAuthor" />
                     <xsd:element name="RespPerson" type="comp:ResponsiblePersonCF1R" maxOccurs="unbounded" />-->
                     <xsd:element name="Footer" type="comp:Footer" />
                  </xsd:sequence>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Report" type="xsd:base64Binary" id="Sign2"/>
            <xsd:element name="Report2" type="xsd:base64Binary" id="Sign3"/>
            <xsd:element name="Input" type="xsd:base64Binary" id="Sign4"/>
         </xsd:sequence>
         <xsd:attribute name="revision" use="required" type="xsd:string" fixed="rev 20220601"/>
         <xsd:attribute name="doc" use="required" type="comp:ComplianceDocumentTag" fixed="LMCCPRF01E"/>
      </xsd:complexType>
   </xsd:element>
   <xsd:complexType name="LMCCPRF01E">
      <xsd:annotation>
         <xsd:documentation>LMCC Performance Compliance</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="Section_Info" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]A. General Information [/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="Info01_ProjectName" type="com:ProjectName" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Project Name</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Info02_RunDescription" type="comp:RunDescription" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Run Title</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Info03_ProjectStreetAddress" type="comp:ProjectStreetAddress" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Project Location</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Info04_ProjectCity" type="comp:ProjectCity" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">City</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Info05_Title24StandardsVersion" type="comp:Title24StandardsVersion" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Standards Version</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Info06_ProjectZipcode" type="comp:ProjectZipcode" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Zip code</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Info07_ComplianceSoftwareVersion" type="comp:ComplianceSoftwareVersion" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Compliance Software (version)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Info07b_ComplianceSoftwareVersionDetail" type="comp:ComplianceSoftwareVersionDetail"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Info07a_Vendor" type="comp:Vendor"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Info07c_Build" type="comp:Build"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Info08_ClimateZone" type="com:ClimateZone" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Climate Zone</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Info09_Azimuth" type="dtyp:Azimuth" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Building Orientation (deg)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Info10_TopLevelOccupancy"  minOccurs="1" maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Building Type(s)</xsd:documentation>
                     </xsd:annotation>
                     <xsd:simpleType>
                        <xsd:restriction base="com:TopLevelOccupancy">
                           <xsd:enumeration value="AlteredOccupancy"/>
                           <xsd:enumeration value="Healthcare"/>
                           <xsd:enumeration value="HotelMotel"/>
                           <xsd:enumeration value="LowRiseResidential"/>
                           <xsd:enumeration value="MixedOccupancy"/>
                           <xsd:enumeration value="Nonresidential"/>
                           <xsd:enumeration value="OtherOccupancy"/>
                           <xsd:enumeration value="RelocableSchoolBuildingAllZones"/>
