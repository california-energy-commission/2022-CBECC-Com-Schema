         <xsd:element name="Section_C22Result8" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]C8. ENERGY USE INTENSITY (EUI) [/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="C22Result8Col1Heading" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Standard Design (kBtu/ft[d:sup]2[/d:sup] / yr)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C22Result8Col2Heading" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Proposed Design (kBtu/ft[d:sup]2[/d:sup] / yr)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C22Result8Col3Heading" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Margin (kBtu/ft[d:sup]2[/d:sup] / yr)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C22Result8Col4Heading" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Margin Percentage</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C22Result8GEui" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">GROSS EUI[d:sup]1[/d:sup]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C22Result8GEui1_StandardDesignGrossEUI" type="comp:StandardDesignGrossEUI" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">result = (((C22Result7EnergyUseTotal1_EnergySumStandard + C22Result7PV1_EnergySumStandard) x 3412) + (C22Result7EnergyUseTotal4_EnergySumStandardMBtu  x 1000)) / Total Building Area)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C22Result8GEui2_ProposedDesignGrossEUI" type="comp:ProposedDesignGrossEUI" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">result = (((C22Result7EnergyUseTotal2_EnergySumProposed + C22Result7PV2_EnergySumProposedPhotovoltaic) x 3412) + (C22Result7EnergyUseTotal5_EnergySumProposedMBtu  x 1000)) / Total Building Area)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C22Result8GEui3_MarginGrossEUI" type="comp:MarginGrossEUI" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">result = C22Result8GEui1_StandardDesignGrossEUI - C22Result8GEui2_ProposedDesignGrossEUI</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C22Result8GEui4_MarginPctGrossEUI" type="comp:MarginPctGrossEUI" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">result =( (C22Result8GEui1_StandardDesignGrossEUI - C22Result8GEui2_ProposedDesignGrossEUI) / C22Result8GEui1_StandardDesignGrossEUI ) x 100</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C22Result8NEui" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NET EUI[d:sup]1[/d:sup]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C22Result8NEui1_StandardDesignNetEUI" type="comp:StandardDesignNetEUI" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">result = ((C22Result7EnergyUseTotal1_EnergySumStandard  x 3412) + (C22Result7EnergyUseTotal4_EnergySumStandardMBtu  x 1000)) / Total Building Area)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C22Result8NEui2_ProposedDesignNetEUI" type="comp:ProposedDesignNetEUI" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">result = ((C22Result7EnergyUseTotal2_EnergySumProposed x 3412) + (C22Result7EnergyUseTotal5_EnergySumProposedMBtu  x 1000)) / Total Building Area)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C22Result8NEui3_MarginNetEUI" type="comp:MarginNetEUI" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">result = C22Result8NEui1_StandardDesignNetEUI - C22Result8NEui2_ProposedDesignNetEUI</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C22Result8NEui4_MarginPctNetEUI" type="comp:MarginPctNetEUI" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">result = ((C22Result8NEui1_StandardDesignNetEUI - C22Result8NEui2_ProposedDesignNetEUI) / C22Result8NEui1_StandardDesignNetEUI) x 100</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C22Result8Footnote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:i][d:sup]1[/d:sup] Notes: Gross EUI is Energy Use Total (not including PV)/Total Building Area. Net EUI is Energy Use Total (including PV)/Total Building Area.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_C22Result9" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]C9. UNMET LOAD HOURS[/d:b]</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is required if unmet heating load hours are greater than unmet heating load hour limit OR unmet cooling load hours are greater than unmet cooling load hour limit</xsd:documentation>
               <xsd:appinfo source="CBECCnrXML">
                  <dtyp:CBECCnrXMLterm>This section is required if any combination of conditions is true</dtyp:CBECCnrXMLterm>
                  <dtyp:termConditions>p:Proj:Bldg:ThrmlZn:BypassClgUMLHLimit = 0 AND p:Proj:Bldg:ThrmlZn:ClgUnmetLdHrs GT ( p:Proj:Bldg:ThrmlZn:ClgUMLHLimit + 0.1 )  OR 
If p:Proj:Bldg:ThrmlZn:BypassHtgUMLHLimit = 0 AND p:Proj:Bldg:ThrmlZn:HtgUnmetLdHrs GT ( p:Proj:Bldg:ThrmlZn:HtgUMLHLimit + 0.1 )</dtyp:termConditions>
               </xsd:appinfo>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableUMLHC22" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="C22Result9_01_ThermalZoneName" type="hvac:ThermalZoneName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Thermal Zone Name</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
