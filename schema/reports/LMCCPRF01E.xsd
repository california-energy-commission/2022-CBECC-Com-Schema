                                             <xsd:documentation source="FieldText">Efficiency Unit</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>AirSys:HtgEffUnitRpt Or ZnSys:HtgEffUnitRpt Or VRFSys:HtgEffUnitRpt</dtyp:CBECCnrXMLterm>
