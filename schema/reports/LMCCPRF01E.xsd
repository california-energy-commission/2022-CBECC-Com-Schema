                  </xsd:element>
                  <xsd:element name="EnvFootNote3" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]3[/d:sup]South-Facing is oriented to within 45 degrees of true south, including 45[d:deg/]00'00" west of south (SW), but excluding 45[d:deg/]00'00" east of south (SE),[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="EnvFootNote4" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]4[/d:sup]West-Facing is oriented to within 45 degrees of true west, including 45[d:deg/]00'00" north of west  (NW), but excluding 45[d:deg/]00'00" south of west (SW),[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_CRRC" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]G2a. ROOFING PRODUCT SUMMARY  (NONRESIDENTIAL)[/d:b]</xsd:documentation>
               <xsd:appinfo source="CBECCnrXML">
                  <dtyp:CBECCnrXMLterm>This section is required if all conditions are true</dtyp:CBECCnrXMLterm>
                  <dtyp:termConditions>p:Proj:CompPathEnv == Performance And 
p:ConsAssm:CompatibleSufrType == Roof And
p:ConsAssm:HasCRRCProdID == 1</dtyp:termConditions>
               </xsd:appinfo>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableCRRC" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="CRRC01_RoofAssemblyName" type="env:RoofAssemblyName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Assembly Name</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>ConsAssm:Name</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="CRRC02_RoofSlope" type="env:RoofSlope" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Roof Pitch</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>ConsAssm:RoofSlopeRpt</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="CRRC02a_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Roof Rise (x in 12)</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="CRRC03_SolarReflectanceAged" type="com:SolarReflectanceAged" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Aged Solar Reflectance</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>ConsAssm:CRRCAgedRefl</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="CRRC04_ThermalEmittance" type="com:ThermalEmittance" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Thermal Emittance</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>ConsAssm:CRRCAgedEmit</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="CRRC05_SolarReflectanceIndex" type="env:SolarReflectanceIndex" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">SRI</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>ConsAssm:CRRCSRIRpt</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="CRRC05_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">SRI</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_CRRCmf" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]G2b. ROOFING PRODUCT SUMMARY  (MULTIFAMILY AND COMMON AREAS)[/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableCRRCmf" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
