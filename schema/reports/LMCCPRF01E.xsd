                                       <xsd:element name="HvacDry06a_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Efficiency Unit</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
 				    <xsd:element name="HvacDry06b" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Efficiency</xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:complexType>
                                          <xsd:choice maxOccurs="1">
                                             <xsd:element name="HvacDry06b_EfficiencyAFUE" type="hvac:EfficiencyAFUE" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Efficiency</xsd:documentation>
                                                   <xsd:appinfo source="CBECCnrXML">
                                                      <dtyp:CBECCnrXMLterm>AirSys:HtgEffRpt Or ZnSys:HtgEffRpt Or VRFSys:HtgEffRpt</dtyp:CBECCnrXMLterm>
                                                   </xsd:appinfo>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="HvacDry06b_EfficiencyCOP" type="hvac:EfficiencyCOP" >
                                                <xsd:annotation>
