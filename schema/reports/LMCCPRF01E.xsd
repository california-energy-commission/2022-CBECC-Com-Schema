         <xsd:element name="Section_WaterCompact" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]I7.  WATER HEATING - COMPACT DISTRIBUTION [/d:b]</xsd:documentation>
               <xsd:appinfo source="CBECCnrXML">
                  <dtyp:CBECCnrXMLterm>This section is required If Spc:ResSpcDHWFeatures:CompactDistErrorID = 0  AND Spc:ResSpcDHWFeatures:CompactDistIdx GT 0</dtyp:CBECCnrXMLterm>
               </xsd:appinfo>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableWaterCompact" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="WaterCompact01_ResidentialDwellingUnitDescriptor" type="com:ResidentialDwellingUnitDescriptor" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Dwelling Unit Type</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Spc:Name</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="WaterCompact02_DwellingUnitType" type="com:DwellingUnitType"  minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>DwellingUnitBedroomCnt</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="WaterCompact03_WaterHeatingSystemName" type="hvac:WaterHeatingSystemName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Water Heating System Name</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Spc:ResDHWSysRef</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="WaterCompact04_WaterHeaterToBath" type="comp:WaterHeaterToBath" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Master Bath distance of furthest fixture to Water Heater (ft)</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>Spc:ResSpcDHWFeatures:MasterBathFxtrDistance</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="WaterCompact04_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Master Bath distance of furthest fixture to Water Heater (ft)</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="WaterCompact05_WaterHeaterToKitchen" type="comp:WaterHeaterToKitchen" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Kitchen distance of furthest fixture to Water Heater (ft)</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>Spc:ResSpcDHWFeatures:KitFxtrDistance</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="WaterCompact05_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Kitchen distance of furthest fixture to Water Heater (ft)</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="WaterCompact06_WaterHeaterToFurthest3rdFixture" type="comp:WaterHeaterToFurthest3rdFixture" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Furthest Third furthest fixture to Water Heater (ft)</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>Spc:ResSpcDHWFeatures:ThirdRoomFxtrDistance</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="WaterCompact06_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Furthest Third furthest fixture to Water Heater (ft)</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="WaterCompact07_CompactFactor" type="comp:CompactFactor" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Compactness Factor</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="WaterCompact07_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Compactness Factor</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="WaterCompact08_CompactDistributionVerificationRequired" type="comp:CompactDistributionVerificationRequired" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">HERS Verification</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>Spc:ResSpcDHWFeatures:CompactDistIdx = 2 result = true; else false</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="WaterCompact08_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">HERS Verification</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="WaterCompact08a_CompactDistributionType" type="hvac:CompactDistributionType" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                             <xsd:documentation source="AdditionalRequirements">Compact Distribution</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="WaterCompact08a_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                             <xsd:documentation source="AdditionalRequirements">Compact Distribution</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="WaterCompact09_WaterHeaterWeightedDistance" type="comp:WaterHeaterWeightedDistance" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                             <xsd:documentation source="AdditionalRequirements">Weighted Distance</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="WaterCompact09_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="WaterCompact10_WaterHeaterQualificationDistance" type="comp:WaterHeaterQualificationDistance" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                             <xsd:documentation source="AdditionalRequirements">Qualification Distance</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="WaterCompact10_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_WaterDWHR" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">I8. WATER HEATING - DRAIN WATER HEAT RECOVERY</xsd:documentation>
               <xsd:appinfo source="CBECCnrXML">
                  <dtyp:CBECCnrXMLterm>This section is required If Spc:NumResDWHRSysRefs GT 0 AND Spc:TotResDWHRSysShowers GT 0</dtyp:CBECCnrXMLterm>
               </xsd:appinfo>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableDWHR" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="WaterDWHR01_ResidentialDwellingUnitDescriptor" type="com:ResidentialDwellingUnitDescriptor" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Dwelling Unit Type</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>DwellUnitType:Name</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="WaterDWHR01a_DwellingUnitType" type="com:DwellingUnitType"  minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Spc:ResSpcDHWFeatures:DwellingUnitBedroomCnt</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="WaterDWHR02_DWHR_Name" type="comp:DWHR_Name" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">DHW System and DWHR Names</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">This is concatenation of the DWHR system and DWHR name</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Spc:ResSpcDHWFeatures:ResDWHRSysRef[]</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="WaterDWHR03_DWHR_Configuration" type="comp:DWHR_Configuration" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Installation Configuration</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Spc:ResSpcDHWFeatures:ResDWHRSysConfig[]</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="WaterDWHR04_DWHR_DrainCount" type="comp:DWHR_DrainCount" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Shower Drains</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Spc:ResSpcDHWFeatures:ResDWHRSysShowers[]</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="WaterDWHR05_DWHR_HERS_VerificationRequired" type="comp:DWHR_HERS_VerificationRequired" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">HERS Verification</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>"Required" else "n/a"</dtyp:CBECCnrXMLterm>
                                                <dtyp:termConditions>If Spc:ResSpcDHWFeatures:NumResDWHRSysDevs = 1 then "Required" else "n/a"</dtyp:termConditions>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="WaterDWHR05_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">HERS Verification</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="WaterDWHR06_DWHR_RatedEffectiveness" type="comp:DWHR_RatedEffectiveness" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                             <xsd:documentation source="AdditionalRequirements">CSA Rated Efficiency</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="WaterDWHR06_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                             <xsd:documentation source="AdditionalRequirements">CSA Rated Efficiency</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="WaterDWHR07_DWHR_PercentShowerServed" type="comp:DWHR_PercentShowerServed" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                             <xsd:documentation source="AdditionalRequirements">Percent of showers served by DWHR device</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="WaterDWHR07_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                             <xsd:documentation source="AdditionalRequirements">Percent of showers served by DWHR device</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_ProcGarage" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]J1. ENCLOSED PARKING GARAGE[/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableProcGarage" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="ProcGarage01_ExhaustFanName" type="hvac:ExhaustFanName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Garage Exhaust System Name</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>If Bldg:AirSys:ExhSysType = ParkingGarage, then report Bldg:AirSys:Name  Else If Bldg:ZnSys:ExhSysType = ParkingGarage, then report Bldg:ZnSys:Name</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="ProcGarage02_ExhaustFlowRate" type="hvac:ExhaustFlowRate" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Design Exhaust Flow Rate (cfm)</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Bldg:AirSys:ExhFanCap  Or Bldg:ZnSys:ExhFanCap</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="ProcGarage03_AirFlowMinimum" type="hvac:AirFlowMinimum" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Minimum Exhaust Flow Rate (cfm)</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Bldg:AirSys:ExhFlowMin  Or Bldg:ZnSys:ExhFlowMin</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="ProcGarage04_FanPowerModeled" type="hvac:FanPowerModeled" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Fan Power (Watts)</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Bldg:AirSys:ExhFanPwr  Or Bldg:ZnSys:ExhFanPwr</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="ProcGarage05_MonitoringCO" type="comp:MonitoringCO" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">CO Control Yes/No</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Bldg:AirSys:COCtrlRpt  Or Bldg:ZnSys:COCtrlRpt</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_ProcKitch" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]J2. COMMERCIAL KITCHENS [/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableProcKitch" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="ProcKitch01_SpaceName" type="com:SpaceName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Space Name</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>If Bldg:Story:Spc:Type="Kitchen/Food Preparation Area", then report Bldg:Story:Spc:Name</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="TableKitchHoods" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"></xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                                <xsd:complexType>
                                                   <xsd:sequence>
                                                      <xsd:element name="ProcKitch02_KitchenExhaustHoodStyle" type="com:KitchenExhaustHoodStyle" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Exhaust Hood Style</xsd:documentation>
                                                            <xsd:appinfo source="CBECCnrXML">
                                                               <dtyp:CBECCnrXMLterm>Bldg:Story:Spc:KitExhHoodStyle[x]</dtyp:CBECCnrXMLterm>
                                                            </xsd:appinfo>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:choice maxOccurs="1">
                                                         <xsd:element name="ProcKitch03_KitchenExhaustHoodDuty" type="com:KitchenExhaustHoodDuty" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Exhaust Hood Duty</xsd:documentation>
                                                               <xsd:appinfo source="CBECCnrXML">
                                                                  <dtyp:CBECCnrXMLterm>Bldg:Story:Spc:KitExhHoodDuty[x]</dtyp:CBECCnrXMLterm>
                                                               </xsd:appinfo>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                         <xsd:element name="ProcKitch03_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Exhaust Hood Duty</xsd:documentation>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                      </xsd:choice>
                                                      <xsd:choice maxOccurs="1">
                                                         <xsd:element name="ProcKitch04_KitchenExhaustHoodLength" type="com:KitchenExhaustHoodLength" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Exhaust Length</xsd:documentation>
                                                               <xsd:appinfo source="CBECCnrXML">
                                                                  <dtyp:CBECCnrXMLterm>Bldg:Story:Spc:KitExhHoodLen[x]</dtyp:CBECCnrXMLterm>
                                                               </xsd:appinfo>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                         <xsd:element name="ProcKitch04_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Exhaust Length</xsd:documentation>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                      </xsd:choice>
                                                      <xsd:choice maxOccurs="1">
                                                         <xsd:element name="ProcKitch05_KitchenExhaustHoodFlow" type="com:KitchenExhaustHoodFlow" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Exhaust Flow Rate (cfm)</xsd:documentation>
                                                               <xsd:appinfo source="CBECCnrXML">
                                                                  <dtyp:CBECCnrXMLterm>Bldg:Story:Spc:KitExhHoodFlow[x]</dtyp:CBECCnrXMLterm>
                                                               </xsd:appinfo>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                         <xsd:element name="ProcKitch05_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Exhaust Flow Rate (cfm)</xsd:documentation>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                      </xsd:choice>
                                                   </xsd:sequence>
                                                </xsd:complexType>
                                             </xsd:element>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_ProcLab" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]J3. LABORATORY/PROCESS EXHAUST[/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableProcLab" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="ProcLab01_SpaceName" type="com:SpaceName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Space Name</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>If Bldg:Story:Spc:SpcFunc =  "Scientific Laboratory Area" then report Bldg:Story:Spc:Name</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="ProcLab02_ExhaustFlowRate" type="hvac:ExhaustFlowRate" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Design Exhaust Flow Rate (cfm)</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Bldg:Story:Spc:ExhFlow</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="ProcLab03_AirFlowMinimum" type="hvac:AirFlowMinimum" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Minimum Exhaust Flow Rate (cfm)</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Bldg:Story:Spc:PropMinExhFlow</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="ProcLab04_LaboratoryFumeHoodLength" type="com:LaboratoryFumeHoodLength" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Total Fume Hood Length (ft)</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Bldg:Story:Spc: LabFumeHoodLen</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="ProcLab05_LaboratoryFumeHoodVerticalSashExhaustFlow" type="com:LaboratoryFumeHoodVerticalSashExhaustFlow" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Exhaust Flow for Fume Hoods with Vertical Sashes (cfm)</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>Bldg:Story:Spc: LabFumeHoodVertSashExhFlow</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="ProcLab05_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Exhaust Flow for Fume Hoods with Vertical Sashes (cfm)</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="ProcLab06_LaboratoryFumeHoodVerticalAutomaticSashControlFraction" type="com:LaboratoryFumeHoodVerticalAutomaticSashControlFraction" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Fraction of Fume Hoods with Vertical Sash with automatic control (%)</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>Bldg:Story:Spc: LabFumeHoodVertAutoSashCtrlFrac</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="ProcLab06_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Fraction of Fume Hoods with Vertical Sash with automatic control (%)</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_LtgInfo" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]K1. INDOOR CONDITIONED LIGHTING GENERAL INFO[/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableLtgInfo" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="LtgInfo01_SpaceFunction" type="com:SpaceFunction" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Occupancy Type[d:sup]1,3[/d:sup]</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Spc:SpcFunc</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="LtgInfo02_SpaceFloorArea" type="com:SpaceFloorArea" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Conditioned Floor Area[d:sup]2[/d:sup] (ft2)</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Spc:CondFlrAreaWithMult</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="LtgInfo03_AllLuminairePowerInArea" type="lit:AllLuminairePowerInArea" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Installed Lighting Power (Watts)</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Spc:CondRegInstLtgPwrWithMult</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="LtgInfo04_PAF_AllowedInArea" type="comp:PAF_AllowedInArea" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Lighting Control Credits (Watts)</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Spc:CondPAFCtrlCredWithMult</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="LtgInfo05Label" minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Additional (Custom) Allowance</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="LtgInfo05_ExtraPowerCalculatedForArea" type="comp:ExtraPowerCalculatedForArea" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Area Category Footnotes (Watts)</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Spc:CondACMTotAllowPwrWithMult</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="LtgInfo06_TailoredPowerInArea" type="comp:TailoredPowerInArea" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Tailored Method (Watts)</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Spc:CondTMTotAllowPwrWithMult</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="LtgInfo01TotalLabel" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Building Totals:</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="LtgInfo02Total_SpacesConditionedArea" type="com:SpacesConditionedArea" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Resullt = SUM ALL LtgInfo02_SpaceFloorArea</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="LtgInfo03Total_LightingPowerInstalledTotalConditioned" type="comp:LightingPowerInstalledTotalConditioned" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Resullt = SUM ALL LtgInfo03_AllLuminairePowerInArea</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="LtgInfo04Total_TotalPAF_AllowedAllAreas" type="comp:TotalPAF_AllowedAllAreas" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Resullt = SUM ALL LtgInfo04_PAF_AllowedInArea</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="LtgInfo05Total_AreaAdditionalLightingPowerAllowedConditioned" type="comp:AreaAdditionalLightingPowerAllowedConditioned" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Resullt = SUM ALL LtgInfo05a_ExtraPowerCalculatedForArea</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="LtgInfo06Total_TailoredTotalLightingPowerAllowedConditioned" type="comp:TailoredTotalLightingPowerAllowedConditioned" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Resullt = SUM ALL LtgInfo05b_TailoredPowerInArea</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="LtgInfoFootNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:i][d:sup]1[/d:sup]See Table 140.6-C[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="LtgInfoFootNote2" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:i][d:sup]2[/d:sup]See LMCC-LTI01--E for unconditioned spaces[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="LtgInfoFootNote3" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:i][d:sup]3[/d:sup]Lighting information for existing spaces modeled is not included in this table[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_LtgSched" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]K2. INDOOR CONDITIONED LIGHTING SCHEDULE[/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="LtgSchedBeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]Luminaire Schedule[/d:b] (includes all permanent installed lighting in conditioned space, and portable lighting over 0.3 w/ft[d:sup]2[/d:sup] in offices)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableLtgSched" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="LtgSchedCol03to06" minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">[d:b]Installed Watts (Conditioned)[/d:b]</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="LtgSched01_LuminaireName" type="lit:LuminaireName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Name or Item Tag</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Proj:Lum:Name</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="LtgSched02_LuminaireDescription" type="lit:LuminaireDescription" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Complete Luminaire Description (i.e. 3-lamp fluorescent troffer, F32T8, one dimmable electronic ballast)</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>Proj:Lum:LumDescRpt</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="LtgSched02_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Complete Luminaire Description (i.e. 3-lamp fluorescent troffer, F32T8, one dimmable electronic ballast)</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="LtgSched03_LuminairePower" type="lit:LuminairePower" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Watts per luminaire</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Proj:Lum:Pwr</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="LtgSched04_LuminaireSpecSource" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">How is Wattage determined</xsd:documentation>
                                          <xsd:appinfo source="displayterm">
                                             <dtyp:displayterm value="SeeOtherSection">According to 130.0(c)</dtyp:displayterm>
                                          </xsd:appinfo>
                                          
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>If Proj:Lum:PwrNA8Default == 1, result = CEC_Default</dtyp:CBECCnrXMLterm>
                                             <dtyp:termConditions>If Proj:Lum:PwrNA8Default == 1, result = CEC_Default 
Else result = SeeOtherSection  End If</dtyp:termConditions>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="comp:LuminaireSpecSource">
                                             <xsd:enumeration value="CEC_Default"/>
                                             <xsd:enumeration value="SeeOtherSection"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:element name="LtgSched05_LuminaireCountByType" type="lit:LuminaireCountByType" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Total Number of Luminaires</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Proj:Lum:CondCntWithMult</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="LtgSched06_LuminairePowerByType" type="lit:LuminairePowerByType" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Installed Watts</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Result = LtgSched03_LuminairePower x LtgSched06_LuminaireCountByType</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Proj:Lum:CondPwrWithMult</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="LtgSchedFootNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:i][d:sup]1[/d:sup]If lighting power densities were used in the compliance model Building Departments will need to check prescriptive forms for Luminaire Schedule details.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_LtgCredits" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]K3. INDOOR CONDITIONED LIGHTING CONTROL CREDITS[/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="LtgCreditsBeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:b]Lighting Control Credits Schedule[/d:b] (includes all lighting controls installed in conditioned space for compliance credit per [d:t24refnolink/]140.6(a)2 and Table 140.6-A)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableLtgCredits" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="LtgCredits01_SpaceName" type="com:SpaceName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Area Description</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Spc:IntLtgSys:ParentSpcName</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="LtgCredits02_SpaceFunction" type="com:SpaceFunction" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Primary Function Area (must meet requirements of Table 140.6-A and 170.2-L)</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Spc:IntLtgSys:ParentSpcFunc</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="LtgCredits03_PAFCreditType" type="comp:PAFCreditType"  minOccurs="0" maxOccurs="unbounded">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Type of Lighting Control</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Spc:IntLtgSys:PAFCredType</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="LtgCredits04_LightingPAF" type="lit:LightingPAF"  minOccurs="0" maxOccurs="unbounded">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Power Adjustment Factor (PAF)</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Spc:IntLtgSys:PAF</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="TableLuminairesCredits" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"></xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                                <xsd:complexType>
                                                   <xsd:sequence>
                                                      <xsd:element name="LtgCredits05_LuminaireName" type="lit:LuminaireName" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Luminaire Item Tag</xsd:documentation>
                                                            <xsd:appinfo source="CBECCnrXML">
                                                               <dtyp:CBECCnrXMLterm>Spc:IntLtgSys:LumRef[]</dtyp:CBECCnrXMLterm>
                                                            </xsd:appinfo>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="LtgCredits06_LuminairePower" type="lit:LuminairePower" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Watts per Luminaire</xsd:documentation>
                                                            <xsd:appinfo source="CBECCnrXML">
                                                               <dtyp:CBECCnrXMLterm>Spc:IntLtgSys:LumPwr[]</dtyp:CBECCnrXMLterm>
                                                            </xsd:appinfo>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="LtgCredits07_LuminaireCountByType" type="lit:LuminaireCountByType" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText"># of Luminaires</xsd:documentation>
                                                            <xsd:appinfo source="CBECCnrXML">
                                                               <dtyp:CBECCnrXMLterm>Spc:IntLtgSys:LumCnt[]</dtyp:CBECCnrXMLterm>
                                                            </xsd:appinfo>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                   </xsd:sequence>
                                                </xsd:complexType>
                                             </xsd:element>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="LtgCredits08_LuminairePowerInArea" type="lit:LuminairePowerInArea" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Lighting Controlled (Watts)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">SUM of all rows in TableLuminaires: LtgCredits06_LuminairePower x LtgCredits07_LuminaireCountByType</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Story:Spc:IntLtgSys:RegInstLtgPwr</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="LtgCredits09_PAF_AllowedInArea" type="comp:PAF_AllowedInArea" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Control Credit (Watts)</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Spc:IntLtgSys:PAFCtrlCred</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="LtgCredits09Total_TotalPAF_AllowedAllAreas" type="comp:TotalPAF_AllowedAllAreas" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Lighting Control Credits (Conditioned) Total (Watts)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Result = SUM ALL  LtgCredits09_PAF_AllowedInArea</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_LtgControls" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]K4. INDOOR CONDITIONED LIGHTING MANDATORY LIGHTING CONTROL [/d:b]</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="LtgControlsCol0102Label" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Building Level Controls</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="LtgControls01_MandatoryBuildingDemandResponseControlRequired" type="comp:MandatoryBuildingDemandResponseControlRequired" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Mandatory Demand Response [d:t24refnolink/]110.12(c)</xsd:documentation>
                        <xsd:appinfo source="CBECCnrXML">
                           <dtyp:CBECCnrXMLterm>Proj:MandatoryLtgCtrlDescRpt[]</dtyp:CBECCnrXMLterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="LtgControls02_MandatoryBuildingShutOffControlRequired" type="comp:MandatoryBuildingShutOffControlRequired" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Shut-Off Controls [d:t24refnolink/]130.1(c) &amp; 160.5(b)4C</xsd:documentation>
                        <xsd:appinfo source="CBECCnrXML">
                           <dtyp:CBECCnrXMLterm>Proj:MandatoryLtgCtrlIsBldgShutOffCtrlRpt</dtyp:CBECCnrXMLterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>                          
                  <xsd:element name="LtgControlsNA" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">See LMCC-LTI01-E for mandatory controls</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">See LMCC-LTI01-E for mandatory controls</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If no data for this section display this message</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="LtgControlsCol0409Label" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Area Level Controls [d:i](includes all lighting controls installed in conditioned space to meet mandatory requirements per [d:t24refnolink/]130.1)[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableLtgControls" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="LtgControls03_SpaceDescription" type="com:SpaceDescription" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Area Description</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Proj:MandatoryLtgCtrlDescRpt[]</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="LtgControls04_SpaceFunction" type="com:SpaceFunction" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Area Category Primary Function Area</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Proj:MandatoryLtgCtrlSpcFuncRpt[]</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="LtgControls05_MandatoryAreaControlsRequired" type="comp:MandatoryAreaControlsRequired" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Area Controls 130.1(a) &amp; 160.5(b)4A</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Proj:MandatoryLtgCtrlIsAreaCtrlRpt[]</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="LtgControls06_MandatoryMultiLevelControlRequired" type="comp:MandatoryMultiLevelControlRequired" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Multi-Level Controls 130.1(b) &amp; 160.5(b)4B</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Proj:MandatoryLtgCtrlIsMultiLvlCtrlRpt[]</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="LtgControls07_MandatoryShutOffControlRequired" type="comp:MandatoryShutOffControlRequired" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Shut-Off Controls 130.1(c) &amp; 160.5(b)4C</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Proj:MandatoryLtgCtrlIsShutOffCtrlRpt[]</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="LtgControls08_MandatoryPrimaryDaylightingControlRequired" type="comp:MandatoryPrimaryDaylightingControlRequired" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Primary Daylighting 130.1(d) &amp; 160.5(b)4D</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Proj:MandatoryLtgCtrlIsPriDayltgCtrlRpt[]</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="LtgControls09_MandatorySecondaryDaylightingControlRequired" type="comp:MandatorySecondaryDaylightingControlRequired" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Secondary Daylighting 140.5(d) &amp; 160.5(b)4D</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Proj:MandatoryLtgCtrlIsSecDayltgCtrlRpt[]</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_TailoredSum" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]K5. TAILORED METHOD: CONDITIONED LIGHTING POWER ALLOWANCE SUMMARY  [/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TailoredSum01_TailoredPowerAllowedGeneral" type="comp:TailoredPowerAllowedGeneral" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">General lighting power allowance</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Result = TailoredGen08Totals_TailoredPowerAllowedGeneral</xsd:documentation>
                        <xsd:appinfo source="CBECCnrXML">
                           <dtyp:CBECCnrXMLterm>Bldg:CondTMBaseGenPwr</dtyp:CBECCnrXMLterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TailoredSum02_PowerAllowedUseLoseTotal" type="comp:PowerAllowedUseLoseTotal" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Additional "use it or lose it"</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Result = SUM all rows (TailoredWall13_PowerAllowedWallDisplay) + SUM all rows (TailoredFloor13_PowerAllowedFloorDisplayOrTask) + SUM all rows (TailoredOrnamental13_PowerAllowedOrnamentalSpecialEffects) + SUM all rows (TailoredValuable13_TailoredPowerAllowedForArea)</xsd:documentation>
                        <xsd:appinfo source="CBECCnrXML">
                           <dtyp:CBECCnrXMLterm>Result = Bldg:CondTMWallDispAllow + Bldg:CondTMCmbdFlrDispTaskAllow + Bldg:CondTMOrnAndSpclEftAllow + Bldg:CondTMVeryValDispAllow</dtyp:CBECCnrXMLterm>
                           <dtyp:termConditions>Result = Bldg:CondTMWallDispAllow + Bldg:CondTMCmbdFlrDispTaskAllow + Bldg:CondTMOrnAndSpclEftAllow + Bldg:CondTMVeryValDispAllow</dtyp:termConditions>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TailoredSum03_TailoredTotalLightingPowerAllowedConditioned" type="comp:TailoredTotalLightingPowerAllowedConditioned" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]Total Allowed Watts using Tailored Method [/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Result = TailoredSum01_TailoredPowerAllowedGeneral + TailoredSum02_PowerAllowedUseLoseTotal</xsd:documentation>
                        <xsd:appinfo source="CBECCnrXML">
                           <dtyp:CBECCnrXMLterm>Result = Bldg:CondTMBaseGenPwr + Bldg:CondTMWallDispAllow + Bldg:CondTMCmbdFlrDispTaskAllow + Bldg:CondTMOrnAndSpclEftAllow + Bldg:CondTMVeryValDispAllow</dtyp:CBECCnrXMLterm>
                           <dtyp:termConditions>Result = Bldg:CondTMBaseGenPwr + Bldg:CondTMWallDispAllow + Bldg:CondTMCmbdFlrDispTaskAllow + Bldg:CondTMOrnAndSpclEftAllow + Bldg:CondTMVeryValDispAllow</dtyp:termConditions>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_TailoredGen" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]K6. TAILORED METHOD: GENERAL LIGHTING POWER ALLOWANCE [/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableTailoredGen" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="TailoredGen01_SpaceDescription" type="com:SpaceDescription" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Area Description</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Spc:Name</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="TailoredGen02_SpaceFunctionTailored" type="comp:SpaceFunctionTailored" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Primary Function Area</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>Spc:SpcFunc</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="TailoredGen02_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Primary Function Area</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="TailoredGen03_SpaceIlluminationLevel" type="lit:SpaceIlluminationLevel" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Illuminance Value (Lux)</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Spc:TMGenlIlumRnd</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="TailoredGen04_RoomCavityRatio" type="com:RoomCavityRatio" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Room Cavity Ratio (RCR)</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Spc:RCR</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="TailoredGen05_TailoredGeneralLPD" type="comp:TailoredGeneralLPD" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Allowed Density (W/ft[d:sup]2[/d:sup])</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Spc:TMBaseGenlLPD</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="TailoredGen06_SpaceFloorArea" type="com:SpaceFloorArea" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Area (ft[d:sup]2[/d:sup])</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Spc:FlrArea</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="TailoredGen07_PowerAllowedUseLoseTotal" type="comp:PowerAllowedUseLoseTotal" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Extra Allowance (Watts)</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Spc:TMTotAllowPwr</dtyp:CBECCnrXMLterm>
                                             <dtyp:termConditions>Result = Bldg:CondTMWallDispAllow + Bldg:CondTMCmbdFlrDispTaskAllow + Bldg:CondTMOrnAndSpclEftAllow + Bldg:CondTMVeryValDispAllow</dtyp:termConditions>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="TailoredGen08_TailoredPowerAllowedForArea" type="comp:TailoredPowerAllowedForArea" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Allowed Watts</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">result = TailoredGen05_TailoredGeneralLPD x  TailoredGen06_SpaceFloorArea</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Proj:Bldg:Story:Spc:TMBaseGenlPwr</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="TailoredGen08Totals_TailoredPowerAllowedGeneral" type="comp:TailoredPowerAllowedGeneral" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Total General Allowance (Watts) CONDITIONED SPACES:</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">result = SUM ALL  TailoredGen08_TailoredPowerAllowedForArea</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_TailoredRCR" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]K7. TAILORED METHOD: ROOM CAVITY RATIO [/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableTailoredRCR" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="TailoredRCR01_SpaceDescription" type="com:SpaceDescription" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Area Description</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Spc:Name</dtyp:CBECCnrXMLterm>
                                             <dtyp:termConditions>Only report  spaces where Spc:CondgType == DirectlyConditioned Or IndirectlyConditioned</dtyp:termConditions>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="TailoredRCR02_SpaceFunctionTailored" type="comp:SpaceFunctionTailored" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Primary Function Area</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>Spc:SpcFunc</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="TailoredRCR02_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Primary Function Area</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="TailoredRCR03_SpaceFloorArea" type="com:SpaceFloorArea" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Area (ft[d:sup]2[/d:sup])</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Spc:Area</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="TailoredRCR04_SpacePerimeter" type="com:SpacePerimeter" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Room Perimeter (ft)</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Spc:Perim</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="TailoredRCR05_Height" type="dtyp:Height" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Room Cavity Height (ft)</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Spc:TaskHgt</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="TailoredRCR06_RoomCavityRatio" type="com:RoomCavityRatio" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Room Cavity Ratio (RCR)</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Spc:RCR</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_TailoredWall" minOccurs="0">
            <xsd:annotation>
