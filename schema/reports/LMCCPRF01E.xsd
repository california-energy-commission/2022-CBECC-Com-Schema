            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableTailoredValuable" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="TailoredValuable01_SpaceDescription" type="com:SpaceDescription" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Area Description</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Spc:Name</dtyp:CBECCnrXMLterm>
                                             <dtyp:termConditions>Reference space If Spc:TMVeryValDispAllow GT 0</dtyp:termConditions>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="TailoredValuable02_SpaceFunctionTailored" type="comp:SpaceFunctionTailored" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Primary Function Area</xsd:documentation>
                                             <xsd:appinfo source="CBECCnrXML">
                                                <dtyp:CBECCnrXMLterm>Spc:SpcFunc</dtyp:CBECCnrXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="TailoredValuable02_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Primary Function Area</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="TailoredValuable03to10Label" minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Allotted Watts</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="TailoredValuable03to05SubLabel" minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">QUALIFYING FLOOR AREA</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="TailoredValuable03_SpaceFloorArea" type="com:SpaceFloorArea" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Area (ft[d:sup]2[/d:sup])</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Spc:FlrArea</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="TailoredValuable04_PowerAllowedVeryValuableDisplay" type="comp:PowerAllowedVeryValuableDisplay" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Allowed Density (W/ft)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">result = 0.55</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>result = 0.55</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="TailoredValuable05_TailoredPowerCalculatedForArea" type="comp:TailoredPowerCalculatedForArea" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Allotted Watts</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">result = TailoredValuable03_SpaceFloorArea  x  TailoredValuable04_PowerAllowedVeryValuableDisplay</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>result = 0.55 x Spc:FlrArea</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="TailoredValuable06to10SubLabel" minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">QUALIFYING DISPLAY CASE AREA</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="TableTailoredCase" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"></xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                                <xsd:complexType>
                                                   <xsd:sequence>
                                                      <xsd:choice maxOccurs="1">
                                                         <xsd:element name="TailoredValuable06_VeryValuableDisplayDescription" type="lit:VeryValuableDisplayDescription" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Description of Display Case</xsd:documentation>
                                                               <xsd:appinfo source="CBECCnrXML">
                                                                  <dtyp:CBECCnrXMLterm>Spc:IntLtgSys:VeryValDispDesc</dtyp:CBECCnrXMLterm>
                                                               </xsd:appinfo>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                         <xsd:element name="TailoredValuable06_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Description of Display Case</xsd:documentation>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                      </xsd:choice>
                                                      <xsd:element name="TailoredValuable07_InteriorLightingSystemName" type="lit:InteriorLightingSystemName" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Interior Lighting System</xsd:documentation>
                                                            <xsd:appinfo source="CBECCnrXML">
                                                               <dtyp:CBECCnrXMLterm>Spc:IntLtgSys:Name</dtyp:CBECCnrXMLterm>
                                                            </xsd:appinfo>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="TailoredValuable08_DisplayCaseArea" type="lit:DisplayCaseArea" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Area of Display Case (ft[d:sup]2[/d:sup])</xsd:documentation>
                                                            <xsd:appinfo source="CBECCnrXML">
                                                               <dtyp:CBECCnrXMLterm>Spc:IntLtgSys:AllowedArea</dtyp:CBECCnrXMLterm>
                                                            </xsd:appinfo>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="TailoredValuable09_TailoredValuablesDisplayCaseLPD" type="comp:TailoredValuablesDisplayCaseLPD" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Allowed Density (W/ft[d:sup]2[/d:sup])</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">result = 8</xsd:documentation>
                                                            <xsd:appinfo source="CBECCnrXML">
                                                               <dtyp:CBECCnrXMLterm>result = 8</dtyp:CBECCnrXMLterm>
                                                            </xsd:appinfo>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="TailoredValuable10_TailoredPowerCalculatedForDisplayCase" type="comp:TailoredPowerCalculatedForDisplayCase" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Allotted Watts</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">TailoredValuable09_TailoredValuablesDisplayCaseLPD *  TailoredValuable08_DisplayCaseArea</xsd:documentation>
                                                            <xsd:appinfo source="CBECCnrXML">
                                                               <dtyp:CBECCnrXMLterm>result = 8 * Spc:IntLtgSys:AllowedArea</dtyp:CBECCnrXMLterm>
                                                            </xsd:appinfo>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                   </xsd:sequence>
                                                </xsd:complexType>
                                             </xsd:element>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="TailoredValuable11_TailoredPowerInArea" type="comp:TailoredPowerInArea" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Design Watts</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Spc:TMVeryValDispDsgn</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="TailoredValuable12_TailoredPowerAllowedForArea" type="comp:TailoredPowerAllowedForArea" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Allowed Watts (smaller of allotted vs design watts)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Result = Smallest (TailoredValuable05_TailoredPowerCalculatedForArea,  TailoredValuable10_TailoredPowerCalculatedForDisplayCase, TailoredValuable11_TailoredPowerInArea )</xsd:documentation>
                                          <xsd:appinfo source="CBECCnrXML">
                                             <dtyp:CBECCnrXMLterm>Spc:TMVeryValDispAllow</dtyp:CBECCnrXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
